            },
            "@type": "owl:Restriction"
        },
        {
            "@id": "arto:length",
            "rdfs:label": "length",
            "skos:example": "\n                # Digital artwork analysis - precise pixel measurement\n                :horizon_line arto:length [\n                    a sdo:QuantitativeValue ;\n                    sdo:value \"847\" ;\n                    sdo:unitCode \"PX\" ;\n                    rdfs:comment \"Horizontal line spanning across landscape painting\"\n                ] .\n\n                # Traditional painting - centimeter measurement  \n                :brushstroke_line arto:length [\n                    a sdo:QuantitativeValue ;\n                    sdo:value \"12.5\" ;\n                    sdo:unitCode \"CM\" ;\n                    rdfs:comment \"Single expressive brushstroke in oil painting\"\n                ] .",
            "rdfs:comment": "Specifies the measurable extent of a linear element within an artwork, representing the distance from one end to the other. In digital analysis, length can be measured in pixels, while in physical artworks it may be expressed in centimeters, inches, or relative proportional units. Length measurement is crucial for compositional analysis, understanding artistic technique, and comparing linear elements across different scales and media. For curved or irregular lines, length represents the total path distance.",
            "rdfs:range": {
                "@id": "sdo:QuantitativeValue"
            },
            "rdfs:domain": {
                "@id": "arto:Line"
            },
            "@type": "owl:ObjectProperty"
        },
