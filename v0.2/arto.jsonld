            },
            "owl:onProperty": {
                "@id": "arto:relatedToEvent"
            },
            "@type": "owl:Restriction"
        },
        {
            "@id": "arto:Event",
            "rdfs:label": "Event",
            "skos:example": "\n          << :Guernica arto:sketchedBy :Picasso >> a arto:Event ;\n              arto:hasTime :t_1937_Spring ; arto:hasLocation :ParisStudio ;\n              dc:type \"Sketching\".\n\n          << :Guernica dc:creator :Picasso >> a arto:Event ;\n              arto:hasTime :t_1937 ; arto:hasLocation :ParisStudio ;\n              dc:type \"Creation\".\n          << :Georgia_OKeeffe arto:movedResidenceTo :Abiquiu_Home >> a arto:Event ;\n            arto:hasTime :t_1949 ; arto:hasLocation :Abiquiu_NewMexico ;\n            dc:type \"ResidenceMove\".\n          ",
            "rdfs:comment": "\nRepresents a temporally and spatially situated occurrence involving agents and locations.\nThis class extends the Event Ontology and aligns conceptually with CIDOC CRM’s E5_Event,\nsupporting cultural heritage interoperability. Event instances may cover artistic activities\n(creation, sketching, exhibition), artist life events (residence changes, collaborations),\nas well as historical or mythological occurrences referenced in artworks. Event categories\ncan be expressed using rdf:type (e.g., Creation, Exhibition, ResidenceMove).\n",
            "rdfs:subClassOf": [
                {
                    "@id": "_:b102"
                },
                {
                    "@id": "_:b103"
                },
                {
                    "@id": "_:b104"
                },
