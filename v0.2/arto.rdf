  <owl:Ontology rdf:about="http://w3id.org/arto">
    <owl:imports rdf:resource="http://www.cidoc-crm.org/cidoc-crm/"/>
    <owl:imports rdf:resource="http://www.w3.org/2004/02/skos/core"/>
    <owl:imports rdf:resource="http://xmlns.com/foaf/spec/"/>
    <owl:imports rdf:resource="https://schema.org/"/>
    <cc:attributionURL rdf:resource="http://w3id.org/arto"/>
    <dc:description xml:lang="en">The Artwork Object Ontology (ARTO) is a comprehensive ontology designed for describing and contextualising artworks. It consists of two main components: the Artwork Descriptive Model and the Artwork Contextual Model. ARTO aims to provide a structured and detailed representation of artworks, facilitating tasks such as artwork captioning and information retrieval.</dc:description>
    <dc:issued>2024-04-10</dc:issued>
    <dc:license rdf:resource="https://github.com/SocialMachineLab/ArtworkOntology/blob/main/License"/>
    <dc:modified>2025-08-05</dc:modified>
    <dc:title xml:lang="en">Artwork Object Ontology</dc:title>
    <ns0:preferredNamespacePrefix>arto</ns0:preferredNamespacePrefix>
    <rdfs:label xml:lang="en">Artwork Object Ontology (ARTO)</rdfs:label>
    <owl:versionInfo>0.2</owl:versionInfo>
  </owl:Ontology>

