  <rdf:Description rdf:about="http://w3id.org/arto#Metric">
    <rdfs:label>Metric</rdfs:label>
    <skos:example>
               :guernica a arto:Artwork ;
                 arto:hasMetric :guernica_MainColour .

               :guernica_MainColour a arto:MainColour ;
                 arto:hasColour :greyTone_Colour .

               :starryNight a arto:Artwork ;
                 arto:hasMetric :starryNight_Balance .

               :starryNight_Balance a arto:BalanceOfComposition ;
                 arto:value 0.72 .

               :sunflowers a arto:Artwork ;
                 arto:hasMetric :sunflowers_Emotion .

               :sunflowers_Emotion a arto:EmotionIntensity ;
                 arto:level "high" .
            </skos:example>
    <rdfs:comment>Represents a quantifiable measurement or characteristic of an artwork,
      encompassing statistical information about basic elements such as colour statistics, emotion
      intensity, balance of composition, and other visual elements, to facilitate structural
      analysis
      and comparison of artworks.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://w3id.org/arto#Space">
