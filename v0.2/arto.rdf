  <owl:Class rdf:about="http://w3id.org/arto#StyleType">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
