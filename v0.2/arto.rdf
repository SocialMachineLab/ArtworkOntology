        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Captures the representation of depth, and spatial organisation within artworks. </rdfs:comment>
    <skos:example>
               :handscroll_Space a arto:Space ;
               dc:type &quot;isometric / axonometric&quot; .
                
              :Field_Space a arto:Space ;
                dc:type &quot;flat / shallow space&quot; .
            </skos:example>
    <rdfs:label>Space</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Sponsor">
