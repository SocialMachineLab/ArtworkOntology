  <owl:Class rdf:about="http://vocab.getty.edu/aat/300163343">
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Agent">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://xmlns.com/foaf/spec/Agent">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasLocation"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Location"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents individuals or organisations involved in artwork creation, curation, or related activities. This class extends foaf:Agent with art-specific roles and relationships. Agents can be artists, curators, collectors, institutions, or other entities. </rdfs:comment>
    <rdfs:label>Agent</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Artist">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Person"/>
    <rdfs:comment>The person or group who create or contribute the artwork.</rdfs:comment>
    <rdfs:label>Artist</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Artwork">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://schema.org/CreativeWork">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/contributor"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/creator"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Artist"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/isPartOf"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Series"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#containsScene"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Scene"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#genre"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#GenreType"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasConnotation"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Connotation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#medium"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#MediumType"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#relatedToEvent"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Event"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#style"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#StyleType"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#technique"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#TechniqueType"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#currentLocation"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
        <owl:onClass rdf:resource="http://w3id.org/arto#Location"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://schema.org/height"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
        <owl:onClass rdf:resource="https://schema.org/QuantitativeValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://schema.org/width"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
        <owl:onClass rdf:resource="https://schema.org/QuantitativeValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/description"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/title"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.europeana.eu/schemas/edm/isShownBy"/>
        <owl:maxQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:maxQualifiedCardinality>
        <owl:onDataRange rdf:resource="http://www.w3.org/2001/XMLSchema#anyURI"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents a created object intended to be aesthetically pleasing or convey artistic meaning. This class encompasses various art forms including paintings, sculptures, photographs, and digital art. Unlike CIDOC-CRM's E22_Human-Made_Object which covers all manufactured items, arto:Artwork specifically focuses on objects created with artistic intent. The class inherits essential properties from schema:CreativeWork (creator, dateCreated, description) while adding art-specific constraints such as mandatory visual representation (edm:isShownBy) and current location tracking. Each artwork must contain at least one scene and have defined dimensions, supporting both traditional and contemporary art analysis.</rdfs:comment>
    <rdfs:label>Artwork</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Series">
            <rdfs:subClassOf>
              <owl:Class rdf:about="https://schema.org/CreativeWorkSeries">
              </owl:Class>
            </rdfs:subClassOf>

            <rdfs:subClassOf>
              <owl:Restriction>
                <owl:onProperty rdf:resource="http://purl.org/dc/terms/creator"/>
                <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Agent"/>
              </owl:Restriction>
            </rdfs:subClassOf>

            <rdfs:comment>A collection of related artworks that share common characteristics, themes, or purposes, including:
                - Thematic series
                - Chronological series
                - Technique-based series
                - Subject-based series
                - Experimental series
                - Documentary series</rdfs:comment>
            <rdfs:label>Series</rdfs:label>
          </owl:Class>
          
  <owl:Class rdf:about="http://w3id.org/arto#BalanceOfComposition">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>Used to assess and describe the visual equilibrium of an artwork's composition. This metric captures how elements are arranged in terms of symmetry, asymmetry, and spatial distribution of visual weight. It can be applied in qualitative analysis (e.g., identifying harmonious vs. dynamic arrangements) or in quantitative evaluation when assigning a balance score to artworks.</rdfs:comment>
    <rdfs:label>BalanceOfComposition</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Character">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Person"/>
    <rdfs:comment>Represents persons, figures, or beings depicted within an artwork.</rdfs:comment>
    <rdfs:label>Character</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Colour">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#VisualElement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasColourTerm"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#ColourTerm"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasColourValue"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#ColourValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents colour usage in artworks with precise technical specifications and art historical context. This class provides both computational colour analysis (RGB, HSV values) and art theoretical foundations, aligning with Getty AAT's colour concepts while maintaining specific properties for digital analysis. Colour instances can express symbolic meanings and emotional content, supporting both technical image processing and interpretive art analysis.</rdfs:comment>
    <skos:example>
            :c_vermilion a arto:Colour ;
              arto:hasColourTerm &lt;http://vocab.getty.edu/aat/300266400&gt; ;  # vermilion
              arto:hasColourValue [
                a arto:ColourValue ;
                arto:colourSystem &quot;RGB&quot; ;
                qudt:vector (227,66,52)  ] .


            :c_bluePeace a arto:Colour ;
              arto:hasColourTerm &lt;http://vocab.getty.edu/aat/300129361&gt; ;  # blue
              arto:hasColourValue [
                a arto:ColourValue ;
                arto:colourSystem &quot;HSV&quot; ;
                qudt:vector (197,76,100)
              ] ;
            </skos:example>
    <rdfs:label>Colour</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#ColourHistogram">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>A metric that captures the distribution of colours within an artwork. It represents how frequently each colour or colour range occurs, typically in the form of a histogram or similar quantitative structure. This metric can be used for tasks such as palette analysis, artwork comparison</rdfs:comment>
    <rdfs:label>ColourHistogram</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#ColourTerm">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
                    <owl:hasValue>
                      <owl:NamedIndividual rdf:about="http://vocab.getty.edu/aat/300131647">
                      </owl:NamedIndividual>
                    </owl:hasValue>

                  </owl:Restriction>
                </rdf:first>

                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:comment>Represents standardised colour terminology from controlled vocabularies, art historical literature, and established colour naming systems. This class provides semantic interoperability by linking colour instances to authoritative colour classifications, enabling consistent colour identification across institutions and research projects. Colour terms encompass basic colour names, specific pigment designations, cultural colour concepts, and technical colour classifications. The class primarily aligns with Getty AAT's colour hierarchy while supporting additional colour vocabularies and historical colour terminology.</rdfs:comment>
    <rdfs:label>ColourTerm</rdfs:label>
    <skos:example>
                   - Basic colour terms: aat:red, aat:blue, aat:yellow, aat:green
                     # Fundamental colour categories from Getty AAT
                   - Specific pigment terms: aat:ultramarine_blue, aat:vermillion, aat:cadmium_yellow
                     # Historical and contemporary pigment names
                   - Cultural colour terms: aat:royal_purple, aat:imperial_yellow, aat:sacred_red
                     # Colours with cultural or symbolic significance
                   - Technical colour terms: aat:primary_colours, aat:complementary_colours, aat:earth_tones
                     # Colour theory and technical classifications
                   - Historical colour terms: aat:tyrian_purple, aat:lapis_lazuli_blue, aat:gold_leaf
                     # Historically significant colourants and materials
                   - Descriptive colour terms: aat:warm_colours, aat:cool_colours, aat:neutral_tones
                     # Colour temperature and mood classifications
                   </skos:example>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#ColourValue">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://qudt.org/schema/qudt/vector"/>
        <owl:allValuesFrom rdf:resource="http://qudt.org/schema/qudt/Vector"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#colourSystem"/>
        <owl:allValuesFrom>
          <rdfs:Datatype>
            <owl:oneOf>
              <rdf:List>
                <rdf:first>CMYK</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>Greyscale</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first>HSV</rdf:first>
                        <rdf:rest>
                          <rdf:List>
                            <rdf:first>Lab</rdf:first>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:first>RGB</rdf:first>
                                <rdf:rest>
                                  <rdf:List>
                                    <rdf:first>XYZ</rdf:first>
                                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                  </rdf:List>
                                </rdf:rest>

                              </rdf:List>
                            </rdf:rest>

                          </rdf:List>
                        </rdf:rest>

                      </rdf:List>
                    </rdf:rest>

                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </owl:oneOf>

          </rdfs:Datatype>
        </owl:allValuesFrom>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the precise numeric specification of colours using various colour models and measurement systems. This class captures quantitative colour data for digital analysis, colour matching and reproduction studies. Colour values enable objective colour comparison, automated colour analysis, and integration with digital image processing workflows. The class supports multiple colour spaces to accommodate different analytical needs and technical requirements.</rdfs:comment>
    <rdfs:label>ColourValue</rdfs:label>
    <skos:example>
                    - RGB specification: :red_primary rdf:type arto:ColourValue ;
                      arto:colourSystem &quot;RGB&quot; ; qudt:vector (255,0,0) ;
                    - HSV specification: :golden_yellow rdf:type arto:ColourValue ;
                      arto:colourSystem &quot;HSV&quot; ; qudt:vector (45,85,95) ;
                    - CMYK print colours: :prussian_blue rdf:type arto:ColourValue ;
                      arto:colourSystem &quot;CMYK&quot; ; qudt:vector (100,85,0,25) ;
                    - LAB colour measurement: :skin_tone rdf:type arto:ColourValue ;
                        arto:colourSystem &quot;Lab&quot; ; qudt:vector (75,8,15) ;
                    </skos:example>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Composition">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#VisualElement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/type"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the arrangement and organisation of visual elements within artworks, fundamental to art historical analysis and aesthetic evaluation. This class captures compositional strategies, balance, and spatial relationships that structure artistic meaning.
                 Currently ARTO includes common compositions types: symmetry, asymmetry, radial balance, rule of thirds, golden ratio, and dynamic composition.
                 </rdfs:comment>
    <rdfs:label>Composition</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Connotation">
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://www.cidoc-crm.org/cidoc-crm/E28_Conceptual_Object">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasMetric"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Metric"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the implied meanings of artwork, includeing themes, symbolic meanings, and
      emotions conveyed by an artwork.</rdfs:comment>
    <rdfs:label>Connotation</rdfs:label>
    <skos:example>
                     - Symbolic meaning: :dove_symbolism rdf:type arto:Symbolism .
                     - Emotional content: :melancholy_mood rdf:type arto:Emotion ; arto:emotionPerspective &quot;artwork&quot;
                     - Historical reference: :revolution_allegory rdf:type arto:Connotation ;
                       dc:description &quot;French Revolution political symbolism&quot;
                     </skos:example>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Emotion">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Connotation"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#expresses"/>
        <owl:allValuesFrom rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#emotionPerspective"/>
        <owl:allValuesFrom>
          <rdfs:Datatype>
            <owl:oneOf>
              <rdf:List>
                <rdf:first>artist</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>artwork</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first>viewer</rdf:first>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </rdf:List>
                    </rdf:rest>

                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </owl:oneOf>

          </rdfs:Datatype>
        </owl:allValuesFrom>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents emotional content or affective responses in artworks.
             Uses MFOEM emotion ontology types and the emotionPerspective property to distinguish 
             between artist's intended emotion, artwork's emotional expression, and viewer's response.
             Emotions are expressed using standardised MFOEM emotion types including basic emotions (happiness, sadness, anger, fear, surprise, disgust) and complex emotions (love, anxiety, hope, despair, serenity). </rdfs:comment>
    <rdfs:label>Emotion</rdfs:label>
    <skos:example>
# Artist's intended emotion
:guernica_antiwar_anger  arto:Emotion ;
    arto:expresses mfoem:MFOEM_000009 ;
    arto:emotionPerspective &quot;artist&quot; ;
    dc:description &quot;Picasso's anger against the brutality of war&quot; .

# Artwork's emotional expression  
:starry_night_turbulence rdf:type arto:Emotion ;
    arto:expresses mfoem:MFOEM_000028 ;
    arto:emotionPerspective &quot;artwork&quot; ;
    dc:description &quot;Anxiety expressed through swirling brushstrokes and dynamic composition&quot; .

# Viewer's emotional response
:mona_lisa_mystery rdf:type  arto:Emotion ;
    arto:expresses mfoem:MFOEM_000167 ;
    arto:emotionPerspective &quot;viewer&quot; ;
    dc:description &quot;Viewer's curiosity evoked by the enigmatic smile&quot; .
</skos:example>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#EmotionIntensity">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>Represents the measured strength of an emotional response evoked by an artwork. 
Values may be expressed on different scales, for example:
- Categorical levels such as low / medium / high
- Numeric ranges (e.g., 0-1, 1-5, or 1-10)
This flexibility allows projects to choose the scale best suited to their analytical or interpretive framework.</rdfs:comment>
    <rdfs:label>EmotionIntensity</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Event">
    <rdfs:subClassOf>
      <owl:Class rdf:about="https://w3id.org/MON/event.owl#Event">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasLocation"/>
        <owl:someValuesFrom rdf:resource="http://w3id.org/arto#Location"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasTime"/>
        <owl:someValuesFrom rdf:resource="http://w3id.org/arto#Time"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://w3id.org/MON/event.owl#agent"/>
        <owl:someValuesFrom rdf:resource="http://w3id.org/arto#Agent"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#relatedToConnotation"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Connotation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/type"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>
Represents a temporally and spatially situated occurrence involving agents and locations.
This class extends the Event Ontology and aligns conceptually with CIDOC CRM’s E5_Event,
supporting cultural heritage interoperability. Event instances may cover artistic activities
(creation, sketching, exhibition), artist life events (residence changes, collaborations),
as well as historical or mythological occurrences referenced in artworks. Event categories
can be expressed using dc:type (e.g., Creation, Exhibition, ResidenceMove).
</rdfs:comment>
    <skos:example>
          &lt;&lt; :Guernica arto:sketchedBy :Picasso &gt;&gt; a arto:Event ;
              arto:hasTime :t_1937_Spring ; arto:hasLocation :ParisStudio ;
              dc:type &quot;Sketching&quot;.

          &lt;&lt; :Guernica dc:creator :Picasso &gt;&gt; a arto:Event ;
              arto:hasTime :t_1937 ; arto:hasLocation :ParisStudio ;
              dc:type &quot;Creation&quot;.
          &lt;&lt; :Georgia_OKeeffe arto:movedResidenceTo :Abiquiu_Home &gt;&gt; a arto:Event ;
            arto:hasTime :t_1949 ; arto:hasLocation :Abiquiu_NewMexico ;
            dc:type &quot;ResidenceMove&quot;.
          </skos:example>
    <rdfs:label>Event</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#GenreType">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
                    <owl:hasValue>
                      <owl:NamedIndividual rdf:about="http://vocab.getty.edu/aat/300056444">
                      </owl:NamedIndividual>
                    </owl:hasValue>

                  </owl:Restriction>
                </rdf:first>

                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the genre of an artwork, such as portrait, landscape, still life, etc. This class allows for the classification of artworks based on their thematic or stylistic content.</rdfs:comment>
    <rdfs:label>GenreType</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Line">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#VisualElement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/type"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#direction"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#length"/>
        <owl:allValuesFrom rdf:resource="https://schema.org/QuantitativeValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="https://schema.org/width"/>
        <owl:allValuesFrom rdf:resource="https://schema.org/QuantitativeValue"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents linear visual elements in artworks. Typical line types may include straight, curved, broken, contour, gestural, structural, implied, and hatching or cross-hatching. These categories are indicative and may be aligned with controlled vocabularies such as Getty AAT where applicable.</rdfs:comment>
    <skos:example>
            :portrait_outline a arto:Line ;
                dc:type &quot;contour&quot; ;
                arto:direction &quot;curved&quot; ;
                arto:length [
                    a sdo:QuantitativeValue ;
                    sdo:value &quot;156&quot; ;
                    sdo:unitCode &quot;PX&quot;
                ] ;
                arto:width [
                    a sdo:QuantitativeValue ;
                    sdo:value &quot;2.1&quot; ;
                    sdo:unitCode &quot;PX&quot;
                ] . </skos:example>
    <rdfs:label>Line</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Location">
    <rdfs:subClassOf rdf:resource="http://www.cidoc-crm.org/cidoc-crm/E53_Place"/>
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://www.opengis.net/ont/geosparql#asWKT"/>
        <owl:allValuesFrom>
          <owl:Class rdf:about="http://www.opengis.net/ont/geosparql#wktLiteral">
          </owl:Class>
        </owl:allValuesFrom>

      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://purl.org/dc/terms/type"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents locations associated with artworks (creation, display, storage, provenance, or depicted scenes). Location type is recorded as free text, e.g., studio, museum, gallery, exhibition venue, storage, auction house, private collection, findspot, city, region, country, fictional place, or depicted place. GeoSPARQL WKT may be provided when precise geometry is known; otherwise names alone are acceptable. Historical changes (renamings/boundaries) can be handled by creating distinct Location instances and linking them to events or times as needed.</rdfs:comment>
    <skos:example>
                # Museum location
                :louvre_museum a arto:Location ;
                    dc:type &quot;museum&quot; ;
                    rdfs:label &quot;Louvre Museum, Paris, France&quot; ;
                    geo:asWKT &quot;POINT(2.3376 48.8606)&quot;^^geo:wktLiteral .

                # Fictional/depicted place
                :garden_of_eden a arto:Location ;
                    dc:type &quot;fictional place&quot; ;
                    rdfs:label &quot;Garden of Eden&quot; ;
                    dc:description &quot;Biblical paradise frequently depicted in medieval and Renaissance art&quot; .

                # City with historical name change
                :leningrad_city a arto:Location ;
                    dc:type &quot;city&quot; ;
                    rdfs:label &quot;Leningrad (1924-1991)&quot; ;
                    geo:asWKT &quot;POINT(30.3351 59.9311)&quot;^^geo:wktLiteral ;
                    dc:description &quot;Former name of St. Petersburg during Soviet period&quot; .
                </skos:example>
    <rdfs:label>Location</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#MainColour">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>Represents the primary or most visually dominant colour of an artwork. 
This value can be derived by computational methods such as colour quantisation, clustering, or histogram analysis. This metric links to a specific Colour instance, which may be described using colour values (RGB, HSV, Lab, etc.) </rdfs:comment>
    <skos:example>
                   - :starryNight_MainColour a arto:MainColour ;
                        arto:hasColour :deepBlue_Colour .

                   - :deepBlue_Colour a arto:Colour ;
                        arto:hasColourValue [
                           a arto:RGBValue ;
                           qudt:vector (18,10,143) ;
                        ] .
                </skos:example>
    <rdfs:label>MainColour</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#MaterialType">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broaderTransitive"/>
                    <owl:hasValue>
                      <owl:NamedIndividual rdf:about="http://vocab.getty.edu/aat/300264091">
                      </owl:NamedIndividual>
                    </owl:hasValue>

                  </owl:Restriction>
                </rdf:first>

                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the material type of an object, such as wood,
      metal, glass, or fabric. This class allows for the classification of objects based on the
      materials used in their construction or representation.</rdfs:comment>
    <rdfs:label>MaterialType</rdfs:label>
    <skos:example>
                  - :wood_material rdf:type arto:MaterialType  .
                  - :metal_material rdf:type arto:MaterialType .
                  - :glass_material rdf:type arto:MaterialType .
                      </skos:example>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#MediumType">
    <rdfs:subClassOf>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first>
                  <owl:Restriction>
                    <owl:onProperty rdf:resource="http://www.w3.org/2004/02/skos/core#broader"/>
                    <owl:someValuesFrom rdf:resource="http://vocab.getty.edu/aat/300163343"/>
                  </owl:Restriction>
                </rdf:first>

                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:comment>Represents the medium of an artwork, such as oil on canvas, watercolour, etc. This class allows for the classification of artworks based on the materials and techniques used in their creation.</rdfs:comment>
    <rdfs:label>MediumType</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Metric">
    <rdfs:comment>Represents a quantifiable measurement or characteristic of an artwork,
      encompassing statistical information about basic elements such as colour statistics, emotion
      intensity, balance of composition, and other visual elements, to facilitate structural
      analysis
      and comparison of artworks.</rdfs:comment>
    <skos:example>
               :guernica a arto:Artwork ;
                 arto:hasMetric :guernica_MainColour .

               :guernica_MainColour a arto:MainColour ;
                 arto:hasColour :greyTone_Colour .

               :starryNight a arto:Artwork ;
                 arto:hasMetric :starryNight_Balance .

               :starryNight_Balance a arto:BalanceOfComposition ;
                 arto:value 0.72 .

               :sunflowers a arto:Artwork ;
                 arto:hasMetric :sunflowers_Emotion .

               :sunflowers_Emotion a arto:EmotionIntensity ;
                 arto:level &quot;high&quot; .
            </skos:example>
    <rdfs:label>Metric</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Object">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasConnotation"/>
        <owl:someValuesFrom rdf:resource="http://w3id.org/arto#Connotation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasCoordinates"/>
        <owl:allValuesFrom rdf:resource="http://qudt.org/schema/qudt/Vector"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasMetric"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Metric"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#material"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#MaterialType"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#relatedTo"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Object"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#relatedToScene"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Scene"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#state"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#descriptor"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#size"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents identifiable and meaningful entities depicted within artworks, composed of various visual elements. Objects serve as the primary content units for artwork analysis, with properties describing their physical state, material composition, and spatial relationships</rdfs:comment>
    <skos:example>
              # Figure in a portrait painting
              :mona_lisa_figure a arto:Object ;
                  rdfs:label &quot;Woman in Portrait&quot; ;
                  arto:size &quot;Large&quot; ;
                  arto:descriptor &quot;seated figure with enigmatic smile&quot; ;
                  arto:state :sitting_state ;
                  arto:hasCoordinates [
                      a qudt:Vector ;
                      qudt:vector (400, 250, 180, 320) # x, y, width, height in pixels
                  ] ;
                  arto:hasConnotation :mystery_emotion ;
                  arto:relatedToScene :portrait_scene .

              # Vase in a still life
              :ceramic_vase a arto:Object ;
                  rdfs:label &quot;Blue Ceramic Vase&quot; ;
                  arto:material aat:ceramic ;
                  arto:size &quot;Medium&quot; ;
                  arto:descriptor &quot;decorative vessel with floral pattern&quot; ;
                  arto:state :intact_state ;
                  arto:hasCoordinates [
                      a qudt:Vector ;
                      qudt:vector (150, 180)  # center position
                  ] ;
                  arto:relatedTo :flower_bouquet ;
                  arto:relatedToScene :table_scene ;
                  arto:hasConnotation :abundance_symbolism .
              </skos:example>
    <rdfs:label>Object</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Organisation">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Agent"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://xmlns.com/foaf/spec/Organization">
      </owl:Class>
    </rdfs:subClassOf>

    <owl:disjointWith rdf:resource="http://w3id.org/arto#Person"/>
    <rdfs:comment>Represents institutional entities involved in artwork creation, curation, preservation, or display, including museums, galleries, art schools, cultural institutions, commercial art enterprises, etc.</rdfs:comment>
    <rdfs:label>Organisation</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Painting">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:comment>A specific type of artwork created using paint or similar media. </rdfs:comment>
    <rdfs:label>Painting</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Person">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Agent"/>
    <rdfs:subClassOf>
      <owl:Class rdf:about="http://xmlns.com/foaf/spec/Person">
      </owl:Class>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#knows"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Person"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Individual human beings involved with artwork may include artists, curators, critics, collectors, conservators, researchers, and other roles.</rdfs:comment>
    <rdfs:label>Person</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Photograph">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:comment>A specific type of artwork created using photography or similar media. </rdfs:comment>
    <rdfs:label>Photograph</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Relevance">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>Measures and describes the significance or importance of elements within an artwork. Relevance may refer to thematic, visual, symbolic, narrative, historical, or compositional importance, and can be expressed by numeric scores, categorical levels, or references to related concepts.</rdfs:comment>
    <skos:example>
                   :lastSupper_RedCloakRelevance a arto:Relevance ;
                     arto:value 0.85 .
               </skos:example>
    <rdfs:label>Relevance</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Scene">
    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#containsObject"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Object"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#containsScene"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Scene"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasConnotation"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Connotation"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasCoordinates"/>
        <owl:allValuesFrom rdf:resource="http://qudt.org/schema/qudt/Vector"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#hasMetric"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Metric"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#relatedToEvent"/>
        <owl:allValuesFrom rdf:resource="http://w3id.org/arto#Event"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#descriptor"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:subClassOf>
      <owl:Restriction>
        <owl:onProperty rdf:resource="http://w3id.org/arto#size"/>
        <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
      </owl:Restriction>
    </rdfs:subClassOf>

    <rdfs:comment>Represents a coherent visual or narrative unit within an artwork that conveys specific meaning or sentiment. A scene aggregates objects and visual elements into meaningful compositions, supporting hierarchical structures where complex artworks contain multiple interconnected scenes. It's similar to ICON's image representations but with explicit support for scene nesting and relationships. Scenes can reference historical, mythological, or fictional events through relatedToEvent, enabling contextual artwork analysis.</rdfs:comment>
    <skos:example>
            # Main scene in &quot;The Last Supper&quot; 
            :last_supper_main_scene a arto:Scene ;
                rdfs:label &quot;The Last Supper Main Scene&quot; ;
                arto:size &quot;Dominant&quot; ;
                arto:descriptor &quot;Indoor dining scene with Jesus and twelve apostles&quot; ;
                arto:hasCoordinates [
                    a qudt:Vector ;
                    qudt:vector (0, 0, 800, 400)  # full painting dimensions
                ] ;
                arto:containsObject :jesus_figure, :judas_figure, :john_figure ;
                arto:containsScene :central_table_scene, :background_architecture_scene ;
                arto:relatedToEvent :biblical_last_supper_event ;
                arto:hasConnotation :betrayal_theme, :sacrifice_symbolism ;
                arto:hasMetric :compositional_balance_metric .

            # Mythological scene
            :birth_of_venus_scene a arto:Scene ;
                rdfs:label &quot;Birth of Venus Scene&quot; ;
                arto:size &quot;Dominant&quot; ;
                arto:descriptor &quot;Goddess emerging from sea foam on shell&quot; ;
                arto:hasCoordinates [
                    a qudt:Vector ;
                    qudt:vector (200, 100, 800, 600)
                ] ;
                arto:containsObject :venus_figure, :scallop_shell, :sea_waves ;
                arto:containsScene :wind_gods_scene, :shore_reception_scene ;
                arto:relatedToEvent :venus_birth_myth ;
                arto:hasConnotation :divine_beauty_theme, :love_symbolism ;
                arto:hasMetric :golden_ratio_composition .
            </skos:example>
    <rdfs:label>Scene</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Sculpture">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:comment>A specific type of artwork created using sculptural techniques, such as carving,
      modelling, or casting.</rdfs:comment>
    <rdfs:label>Sculpture</rdfs:label>
  </owl:Class>

  <owl:Class rdf:about="http://w3id.org/arto#Space">
    <rdfs:subClassOf rdf:resource="http://w3id.org/arto#VisualElement"/>
    <rdfs:subClassOf>
      <owl:Restriction>
