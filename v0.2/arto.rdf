  <owl:ObjectProperty rdf:about="http://w3id.org/arto#containsElement">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isElementOf">
        <owl:inverseOf rdf:resource="http://w3id.org/arto#containsElement"/>
        <rdfs:domain rdf:resource="http://w3id.org/arto#VisualElement"/>
        <rdfs:range>
          <owl:Class>
            <owl:unionOf>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:unionOf>

          </owl:Class>
        </rdfs:range>

        <rdfs:comment>Specifies the object or scene that contains the visual element in the artwork.</rdfs:comment>
        <rdfs:label>isElementOf</rdfs:label>
        <skos:example>
                  - colour elements: :blue_gradient_colour arto:isElementOf :sky_object
                  - Line elements: :diagonal_line arto:isElementOf :mountain_object
                  - Compositional elements: :vertical_composition arto:isElementOf :tree_object</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#VisualElement"/>
    <rdfs:comment>Specifies the visual elements (such as colour, line, composition) contained within an object in the artwork.</rdfs:comment>
    <rdfs:label>containsElement</rdfs:label>
    <skos:example>
                      - colour elements: :sky_object arto:containsElement :blue_gradient_colour
                      - Line elements: :mountain_object arto:containsElement :diagonal_line
                      - Compositional elements: :tree_object arto:containsElement :vertical_composition</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#containsObject">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isObjectOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Object"/>
        <rdfs:range rdf:resource="http://w3id.org/arto#Scene"/>
        <rdfs:comment>Indicates the scene that contains the object in the artwork.</rdfs:comment>
        <rdfs:label>isObjectOf</rdfs:label>
        <skos:example>
                - Landscape scene: :fountain arto:isObjectOf :garden_scene
                - Portrait scene: :chair arto:isObjectOf :interior_scene
                - Still life scene: :fruit_bowl arto:isObjectOf :table_scene</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain rdf:resource="http://w3id.org/arto#Scene"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:comment>Indicates specific objects that are present within a scene of an artwork.</rdfs:comment>
    <rdfs:label>containsObject</rdfs:label>
    <skos:example>
                      - Landscape scene: :garden_scene arto:containsObject :fountain, :bench, :flowers
                      - Portrait scene: :interior_scene arto:containsObject :chair, :table, :book
                      - Still life scene: :table_scene arto:containsObject :fruit_bowl, :wine_glass, :cloth</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#containsScene">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isSceneOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Scene"/>
        <rdfs:range>
          <owl:Class>
            <owl:unionOf>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:unionOf>

          </owl:Class>
        </rdfs:range>

        <rdfs:comment>Specifies the artwork or scene that contains the scene in the artwork.</rdfs:comment>
        <rdfs:label>isSceneOf</rdfs:label>
        <skos:example>
                - Nested scenes: :vendor_scene arto:isSceneOf :market_scene
                - Background scenes: :mountain_scene arto:isSceneOf :landscape_painting</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#Scene"/>
    <rdfs:comment>Identifies distinct scenes within an artwork or within another scene, allowing for hierarchical scene composition.</rdfs:comment>
    <rdfs:label>containsScene</rdfs:label>
    <skos:example>
                      - Nested scenes: :market_scene arto:containsScene :vendor_scene, :customer_scene
                      - Background scenes: :landscape_painting arto:containsScene :mountain_scene, :valley_scene</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#currentLocation">
    <rdfs:subPropertyOf>
      <owl:ObjectProperty rdf:about="http://www.europeana.eu/schemas/edm/currentLocation">
      </owl:ObjectProperty>
    </rdfs:subPropertyOf>

    <rdfs:domain rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Location"/>
    <rdfs:comment>Specifies the present physical location where an artwork is housed, displayed, or stored. This property extends EDM's currentLocation with art-specific constraints ensuring single location assignment while supporting location change tracking. Essential for provenance research, exhibition planning, and artwork accessibility, this property maintains connections to institutional and geographical data through the Location class.</rdfs:comment>
    <rdfs:label>currentLocation</rdfs:label>
    <owl:cardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:cardinality>
    <skos:example>
                      - Museum location: :mona_lisa arto:currentLocation :louvre_museum
                      - Gallery location: :starry_night arto:currentLocation :moma_gallery_room_24
                      </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#expresses">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Emotion"/>
    <rdfs:range rdf:resource="http://purl.obolibrary.org/obo/MFOEM_000001"/>
    <rdfs:comment>Relates arto:Emotion to a specific emotion concept defined in the Mental Functioning Ontology Emotion Module (MFOEM). This property is used to link local emotion representations to controlled vocabulary terms (e.g., joy, sadness, anger, fear) from MFOEM, ensuring consistent and interoperable emotion classification across datasets. In the context of artworks, this enables precise and standardised description of emotions expressed within artistic content.</rdfs:comment>
    <rdfs:label>expresses</rdfs:label>
    <skos:example>
              - :joy_emotion arto:expresses mfoem:MFOEM_000034 ; 
              - :sadness_emotion arto:expresses mfoem:MFOEM_000056 ; 
              - :anger_emotion arto:expresses mfoem:MFOEM_000009 ; 
              - :fear_emotion arto:expresses mfoem:MFOEM_000026 ; 
              </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#genre">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#GenreType"/>
    <rdfs:comment>Genre classification is fundamental to art analysis, which connects artworks to established genre categories from Getty AAT vocabulary while supporting custom genre definitions.</rdfs:comment>
    <rdfs:label>genre</rdfs:label>
    <skos:example>
               - Traditional genres: :mona_lisa arto:genre aat:portrait_paintings
               - Landscape genre: :starry_night arto:genre aat:landscape_paintings  
               - Religious genre: :last_supper arto:genre aat:religious_paintings
               - Still life genre: :sunflowers arto:genre aat:still_life_paintings
               - Abstract genre: :composition_vii arto:genre aat:abstract_paintings
               </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasColourTerm">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Colour"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#ColourTerm"/>
    <rdfs:comment>Links a colour to its corresponding standardised term from controlled vocabularies, establishing semantic connections that enable consistent colour identification, hierarchical classification, and cross-institutional interoperability for cultural heritage documentation and research.</rdfs:comment>
    <skos:example>
                    :colour1 a arto:Colour ;
                      arto:hasColourTerm aat:red .

                    :colour2 a arto:Colour ;
                      arto:hasColourTerm aat:vermillion .

                    :colour3 a arto:Colour ;
                      arto:hasColourTerm aat:imperial_yellow .

                    </skos:example>
    <rdfs:label>hasColourTerm</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasColourValue">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Colour"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#ColourValue"/>
    <rdfs:comment>Associates a colour with its value, allowing for flexible representation of colour information in artworks. This property supports different colour spaces for example RGB and HSV formats, enabling detailed colour analysis and representation.</rdfs:comment>
    <skos:example>
                      - RGB value:
                        :c_vermilion a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSystem &quot;RGB&quot; ;
                            qudt:vector (227 66 52)      # R,G,B in 0–255
                          ] .

                      - HSV value:
                        :c_skyBlue a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSystem &quot;HSV&quot; ;
                            qudt:vector (197 0.76 1.00)  # H in degrees; S,V in 0–1
                          ] .

                      - CMYK value:
                        :c_cadmiumYellow a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSystem &quot;CMYK&quot; ;
                            qudt:vector (0.00 0.09 0.90 0.00)  # C,M,Y,K in 0–1
                          ] .

                      - CIE Lab value with white point:
                        :c_ultramarine a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSystem &quot;Lab&quot; ;
                            qudt:vector (32.4 14.8 -38.1) ;
                            arto:whitePoint arto:D65
                          ] .
                      </skos:example>
    <rdfs:label>hasColourValue</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasConnotation">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isConnotationOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Connotation"/>
        <rdfs:range>
          <owl:Class>
            <owl:unionOf>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                        <rdf:rest>
                          <rdf:Description>
                            <rdf:first rdf:resource="http://w3id.org/arto#VisualElement"/>
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                          </rdf:Description>
                        </rdf:rest>

                      </rdf:Description>
                    </rdf:rest>

                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:unionOf>

          </owl:Class>
        </rdfs:range>

        <rdfs:comment>Associates a connotation with the visual elements, objects, scenes or artwork that express it.</rdfs:comment>
        <rdfs:label>isConnotationOf</rdfs:label>
        <skos:example>
                    - Emotional expression: :anger_emotion arto:isConnotationOf :red_colour
                    - Symbolic meaning: :peace_meaning arto:isConnotationOf :dove_object
                    - Scene mood: :turbulence_emotion arto:isConnotationOf :storm_scene
                    - Artwork meaning: :anti_war_theme arto:isConnotationOf :guernica</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://w3id.org/arto#VisualElement"/>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </rdf:Description>
                    </rdf:rest>

                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#Connotation"/>
    <rdfs:comment>Links visual elements, objects, scenes, or artworks to their symbolic meanings or emotional connotations.The relationship enables multiple interpretation levels while maintaining clear connections between visible elements and their cultural, symbolic, or emotional significance. Similar to ICON's recognizedConcept but with broader domain coverage.</rdfs:comment>
    <rdfs:label>hasConnotation</rdfs:label>
    <skos:example>
              - Emotional expression: :red_colour arto:hasConnotation :anger_emotion
              - Symbolic meaning: :dove_object arto:hasConnotation :peace_meaning
              - Scene mood: :storm_scene arto:hasConnotation :turbulence_emotion
              - Artwork meaning: :guernica arto:hasConnotation :anti_war_theme</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasCoordinates">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://qudt.org/schema/qudt/Vector"/>
    <rdfs:comment>Specifies the spatial location of objects and scenes within the artwork's coordinate system. This property supports spatial analysis, object relationship studies, compositional balance analysis, and digital humanities research requiring precise object localisation.</rdfs:comment>
    <rdfs:label>hasCoordinates</rdfs:label>
    <skos:example>
                        - Digital image coordinates: :main_figure arto:hasCoordinates qudt_Vector_250_300
                    qudt_Vector_250_300 rdf:type qudt:Vector ;
                        - 3D artwork coordinates: :sculpture_detail arto:hasCoordinates qudt_Vector_15_20_5
                    qudt_Vector_15_20_5 rdf:type qudt:Vector ;
                    </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasLocation">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isLocationOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Location"/>
        <rdfs:range>
          <owl:Class>
            <owl:unionOf>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Agent"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Event"/>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:unionOf>

          </owl:Class>
        </rdfs:range>

        <rdfs:comment>Associates a location with the agent or event that it is related to.</rdfs:comment>
        <rdfs:label>isLocationOf</rdfs:label>
        <skos:example>
                - Museum address: :met_nyc_location arto:isLocationOf :metropolitan_museum
                - Gallery location: :london_bankside arto:isLocationOf :tate_modern
                - Studio location: :manhattan_location arto:isLocationOf :warhol_factory
                - Exhibition event: :venice_giardini arto:isLocationOf :venice_biennale_2024</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Agent"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Event"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#Location"/>
    <rdfs:comment>Specifies the physical location of an agent or event related to the artwork.</rdfs:comment>
    <rdfs:label>hasLocation</rdfs:label>
    <skos:example>
                - Museum address: :metropolitan_museum arto:hasLocation :met_nyc_location
                - Gallery location: :tate_modern arto:hasLocation :london_bankside                 
                - Exhibition event: :venice_biennale_2024 arto:hasLocation :venice_giardini</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasMetric">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isMetricOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Metric"/>
        <rdfs:range>
          <owl:Class>
            <owl:unionOf>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Connotation"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                        <rdf:rest>
                          <rdf:Description>
                            <rdf:first rdf:resource="http://w3id.org/arto#VisualElement"/>
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                          </rdf:Description>
                        </rdf:rest>

                      </rdf:Description>
                    </rdf:rest>

                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </owl:unionOf>

          </owl:Class>
        </rdfs:range>

        <rdfs:comment>Associates a metric with the component of an artwork that it is related to.</rdfs:comment>
        <rdfs:label>isMetricOf</rdfs:label>
        <skos:example>
                - Colour metrics: :blue_intensity_75_percent arto:isMetricOf :sky_area
                - Composition balance: :symmetry_score_0.8 arto:isMetricOf :landscape_scene
                - Emotional intensity: :fear_intensity_high arto:isMetricOf :expression_scene</skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Connotation"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
                <rdf:rest>
                  <rdf:Description>
                    <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                    <rdf:rest>
                      <rdf:Description>
                        <rdf:first rdf:resource="http://w3id.org/arto#VisualElement"/>
                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                      </rdf:Description>
                    </rdf:rest>

                  </rdf:Description>
                </rdf:rest>

              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#Metric"/>
    <rdfs:comment>Associates artwork components with quantifiable measurements supporting computational analysis and comparative studies. This property enables integration of statistical colour analysis, compositional balance metrics, emotional intensity measures, and other quantitative assessments with qualitative art historical analysis. The relationship supports both automated artwork analysis and structured comparison studies while maintaining connection to interpretive content.</rdfs:comment>
    <rdfs:label>hasMetric</rdfs:label>
    <skos:example>
                - Colour metrics: :sky_area arto:hasMetric :blue_intensity_75_percent
                - Composition balance: :landscape_scene arto:hasMetric :symmetry_score_0.8
                - Emotional intensity: :expression_scene arto:hasMetric :fear_intensity_high</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#hasTime">
    <owl:inverseOf>
      <owl:ObjectProperty rdf:about="http://w3id.org/arto#isTimeOf">
        <rdfs:domain rdf:resource="http://w3id.org/arto#Time"/>
        <rdfs:range rdf:resource="http://w3id.org/arto#Event"/>
        <rdfs:comment>Associates a time entity with the event that it is related to. The time entity may represent a specific date, a period (e.g., season, decade), or a cultural-historical era, allowing precise or approximate temporal anchoring of events or locations.</rdfs:comment>
        <rdfs:label>isTimeOf</rdfs:label>
        <skos:example>
              - Event timing: :spring2024 arto:isTimeOf :exhibition2024
              - Location historical period: :belle_epoque arto:isTimeOf :paris_studio
              </skos:example>
      </owl:ObjectProperty>
    </owl:inverseOf>

    <rdfs:domain rdf:resource="http://w3id.org/arto#Event"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Time"/>
    <rdfs:comment>Links an event to its temporal specification. The time entity may represent a specific date, a period (e.g., season, decade), or a cultural-historical era, allowing precise or approximate temporal anchoring of events.</rdfs:comment>
    <rdfs:label>hasTime</rdfs:label>
    <skos:example>
               # Specific date
               :exhibition2024 arto:hasTime :date_2024_05_12

               # Period/season
               :venice_biennale2022 arto:hasTime :summer2022

               # Historical era
               :napoleon_coronation arto:hasTime :early19thCentury
              </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#isEventOf">
    <owl:inverseOf rdf:resource="http://w3id.org/arto#relatedToEvent"/>
    <rdfs:domain rdf:resource="http://w3id.org/arto#Event"/>
    <rdfs:range>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:range>

    <rdfs:comment>Associates an event with the artwork or scene that it is related to.</rdfs:comment>
    <rdfs:label>isEventOf</rdfs:label>
    <skos:example>
                - Historical event: :french_revolution_1830 arto:isEventOf :liberty_leading_the_people
                - Biblical event: :biblical_last_supper arto:isEventOf :last_supper
                </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#knows">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Person"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Person"/>
    <rdfs:comment>The person knows persons. This property, equivalent to foaf:knows, enables study of artistic movements, teacher-student relationships, collaborative networks, and cultural exchange patterns essential for understanding art historical development and cross-cultural artistic influence.</rdfs:comment>
    <rdfs:label>knows</rdfs:label>
    <skos:example>
            - Artist relationships: :vincent_van_gogh arto:knows :paul_gauguin
            - Teacher-student: :gustav_klimt arto:knows :egon_schiele
            - Collaborative artists: :pablo_picasso arto:knows :georges_braque</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#material">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#MaterialType"/>
    <rdfs:comment>The material of the Object. It connect to the Getty AAT's material hierarchy, allowing for both standardised and custom material definitions. Not for the artwork's physical medium.</rdfs:comment>
    <skos:example>
                :glass_vase_obj     a arto:Object ; arto:material aat:glass .
                :pewter_plate_obj   a arto:Object ; arto:material aat:pewter .
                :walnut_table_obj   a arto:Object ; arto:material aat:walnut_wood . 
                </skos:example>
    <rdfs:label>material</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#medium">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#MediumType"/>
    <rdfs:comment>Specifies the physical medium of the artwork itself (e.g., oil on canvas, tempera on panel, bronze sculpture). This property aligns with Getty AAT's medium hierarchy, ensuring standardised description of artistic medium.</rdfs:comment>
    <skos:example>
              :painting_001 a arto:Artwork ;
                arto:medium aat:oil_on_canvas .

              :wall_painting_001 a arto:Artwork ;
                arto:medium aat:fresco .

              :bronze_statue_001 a arto:Artwork ;
                arto:medium aat:bronze .
              </skos:example>
    <rdfs:label>medium</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#relatedTo">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:comment>Describes spatial or conceptual relationships between objects within artworks. This extensible property enables specific relationship types (behind, holds, above) through subproperty mechanisms, supporting detailed spatial analysis while maintaining semantic clarity.</rdfs:comment>
    <rdfs:label>relatedTo</rdfs:label>
    <skos:example>
                - Spatial relation: :cup_object arto:rests_on :table_object ;
                  arto:rests_on rdfs:subPropertyOf arto:relatedTo;
                - Interaction: :mother_figure arto:holds :child_figure ;
                  arto:holds rdfs:subPropertyOf arto:relatedTo;
                - Position: :cloud_object arto:above :mountain_object ;
                  arto:above rdfs:subPropertyOf arto:relatedTo ;
                - Position: :tree_object arto:behind :house_object ;
                  arto:behind rdfs:subPropertyOf arto:relatedTo ;</skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#relatedToConnotation">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Event"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Connotation"/>
    <rdfs:comment>Indicates that an event related to some connotations.</rdfs:comment>
    <rdfs:label>relatedToConnotation</rdfs:label>
    <skos:example>
                  ex:NoahDoveEvent rdf:type arto:Event ;
                  arto:relatedToConnotation ex:PeaceSymbolism ;
                </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#relatedToEvent">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Artwork"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://w3id.org/arto#Event"/>
    <rdfs:comment>Connects an artwork or its scene to historical, mythological, or contemporary events depicted or referenced in the work.</rdfs:comment>
    <rdfs:label>relatedToEvent</rdfs:label>
    <skos:example>
                      - Historical event: :liberty_leading_the_people arto:relatedToEvent :french_revolution_1830
                      - Biblical event: :last_supper arto:relatedToEvent :biblical_last_supper
                      </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#relatedToScene">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#Scene"/>
    <rdfs:comment>Links objects to the scenes they appear in or are associated with.</rdfs:comment>
    <rdfs:label>relatedToScene</rdfs:label>
    <skos:example>
                      - Object context: :bread_loaf arto:relatedToScene :market_scene </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#state">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Object"/>
    <rdfs:range rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdfs:comment>Describes the condition, position, or status of objects within artworks, capturing both physical states (damaged, intact) and depicted conditions (motion, rest, emotional states of figures).</rdfs:comment>
    <rdfs:label>state</rdfs:label>
    <skos:example>
               - Physical conditions: :broken_amphora arto:state :damaged_state ;
                 :marble_statue arto:state :weathered_state
               - Motion states: :running_horse arto:state :galloping_state ;
                 :falling_figure arto:state :in_motion_state
               - Positional states: :seated_figure arto:state :sitting_state ;
                 :reclining_venus arto:state :lying_state
               - Emotional states: :weeping_magdalene arto:state :grieving_state ;
                 :laughing_child arto:state :joyful_state
               - Temporal states: :blooming_flower arto:state :spring_state ;
                 :withered_leaves arto:state :autumn_state
               - Functional states: :open_book arto:state :readable_state ;
                 :lit_candle arto:state :burning_state
               </skos:example>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#style">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#StyleType"/>
    <rdfs:comment>Indicates the artistic style or movement of an artwork, such as Impressionism, Cubism, or Baroque. This property links to Getty AAT's style and period classifications, while also allowing for the inclusion of custom style definitions when necessary.</rdfs:comment>
    <skos:example>
              :water_lilies a arto:Artwork ;
                arto:style aat:impressionism .

              :guernica a arto:Artwork ;
                arto:style aat:cubism .

              :baroque_altarpiece a arto:Artwork ;
                arto:style aat:baroque .
              </skos:example>
    <rdfs:label>style</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#technique">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Artwork"/>
    <rdfs:range rdf:resource="http://w3id.org/arto#TechniqueType"/>
    <rdfs:comment>Specifies the artistic methods or processes applied in the creation of the artwork (e.g., fresco, engraving, pointillism). This property connects to Getty AAT’s technique hierarchy, while also supporting the definition of custom techniques not covered by AAT.</rdfs:comment>
    <skos:example>
                :sistine_ceiling a arto:Artwork ;
                  arto:technique aat:fresco .

                :rembrandt_print a arto:Artwork ;
                  arto:technique aat:etching .

                :seurat_painting a arto:Artwork ;
                  arto:technique aat:pointillism .

                :bronze_sculpture a arto:Artwork ;
                  arto:technique aat:lost_wax_casting .
                </skos:example>
    <rdfs:label>technique</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://w3id.org/arto#length">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Line"/>
    <rdfs:range rdf:resource="https://schema.org/QuantitativeValue"/>
    <rdfs:comment>Specifies the measurable extent of a linear element within an artwork, representing the distance from one end to the other. In digital analysis, length can be measured in pixels, while in physical artworks it may be expressed in centimeters, inches, or relative proportional units. Length measurement is crucial for compositional analysis, understanding artistic technique, and comparing linear elements across different scales and media. For curved or irregular lines, length represents the total path distance.</rdfs:comment>
    <skos:example>
                # Digital artwork analysis - precise pixel measurement
                :horizon_line arto:length [
                    a sdo:QuantitativeValue ;
                    sdo:value &quot;847&quot; ;
                    sdo:unitCode &quot;PX&quot; ;
                    rdfs:comment &quot;Horizontal line spanning across landscape painting&quot;
                ] .

                # Traditional painting - centimeter measurement  
                :brushstroke_line arto:length [
                    a sdo:QuantitativeValue ;
                    sdo:value &quot;12.5&quot; ;
                    sdo:unitCode &quot;CM&quot; ;
                    rdfs:comment &quot;Single expressive brushstroke in oil painting&quot;
                ] .</skos:example>
    <rdfs:label>length</rdfs:label>
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="http://www.opengis.net/ont/geosparql#asWKT">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/height">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://schema.org/width">
  </owl:ObjectProperty>

  <owl:ObjectProperty rdf:about="https://w3id.org/MON/event.owl#agent">
  </owl:ObjectProperty>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#colourSystem">
    <rdfs:domain rdf:resource="http://w3id.org/arto#ColourValue"/>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:List>
            <rdf:first>CMYK</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first>Greyscale</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>HSV</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first>Lab</rdf:first>
                        <rdf:rest>
                          <rdf:List>
                            <rdf:first>RGB</rdf:first>
                            <rdf:rest>
                              <rdf:List>
                                <rdf:first>XYZ</rdf:first>
                                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                              </rdf:List>
                            </rdf:rest>

                          </rdf:List>
                        </rdf:rest>

                      </rdf:List>
                    </rdf:rest>

                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </rdf:rest>

          </rdf:List>
        </owl:oneOf>

      </rdfs:Datatype>
    </rdfs:range>

    <rdfs:comment>The colour system of the ColourValue, indicating the model used for its representation. This property supports various colour models such as RGB, HSV, Lab, and CMYK, allowing for flexible colour representation and analysis in artworks. The use of a restricted set of values ensures consistency and interoperability across different systems and applications.</rdfs:comment>
    <skos:example>
                      :vermilion_rgb a arto:ColourValue ;
                      arto:colourSystem &quot;RGB&quot; . 
                      
                      sky_blue_hsv a arto:ColourValue ;
                      arto:colourSystem &quot;HSV&quot; .
                      </skos:example>
    <rdfs:label>colourSystem</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#descriptor">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Provides extensible descriptive framework for capturing object and scene characteristics not covered by specific properties, enabling domain-specific extensions through subproperties. This design addresses the infinite variety of artistic content while maintaining semantic structure. Examples include clothing descriptions for figures, or environmental conditions for scene.</rdfs:comment>
    <rdfs:label>descriptor</rdfs:label>
    <skos:example>
                Portrait1 :clothing :RedDress ;
                :clothing rdfs:subPropertyOf arto:descriptor</skos:example>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#direction">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Line"/>
    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Describes the orientation of linear elements in an artwork.</rdfs:comment>
    <skos:example>
                :horizon_line arto:direction &quot;horizontal&quot; ;


                :dynamic_slash arto:direction &quot;diagonal&quot; ;


                :gesture_stroke arto:direction &quot;sweeping left to right&quot; ;
                </skos:example>
    <rdfs:label>direction</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#emotionPerspective">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Emotion"/>
    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:List>
            <rdf:first>Artist</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first>Artwork</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>Viewer</rdf:first>
                    <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </rdf:rest>

          </rdf:List>
        </owl:oneOf>

      </rdfs:Datatype>
    </rdfs:range>

    <rdfs:comment>Specifies the perspective or viewpoint from which an emotion is interpreted in relation to an artwork. This property distinguishes between emotions intended by the artist, emotions expressed by the artwork itself, emotions experienced by viewers. Values are restricted to: 'Artist' (Artist's intended emotion), 'Artwork' (emotion expressed by the Artwork's formal elements), and 'Viewer' (Viewer's emotional response).</rdfs:comment>
    <rdfs:label xml:lang="en">emotionPerspective</rdfs:label>
    <skos:example>
                          ex:melancholy_emotion arto:emotionPerspective &quot;Artist&quot;^^xsd:string ;
                          ex:serenity_emotion arto:emotionPerspective &quot;Artwork&quot;^^xsd:string ;
                          ex:excitement_emotion arto:emotionPerspective &quot;Viewer&quot;^^xsd:string .
                          </skos:example>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#size">
    <rdfs:domain>
      <owl:Class>
        <owl:unionOf>
          <rdf:Description>
            <rdf:first rdf:resource="http://w3id.org/arto#Object"/>
            <rdf:rest>
              <rdf:Description>
                <rdf:first rdf:resource="http://w3id.org/arto#Scene"/>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
              </rdf:Description>
            </rdf:rest>

          </rdf:Description>
        </owl:unionOf>

      </owl:Class>
    </rdfs:domain>

    <rdfs:range>
      <rdfs:Datatype>
        <owl:oneOf>
          <rdf:List>
            <rdf:first>Dominant</rdf:first>
            <rdf:rest>
              <rdf:List>
                <rdf:first>Large</rdf:first>
                <rdf:rest>
                  <rdf:List>
                    <rdf:first>Medium</rdf:first>
                    <rdf:rest>
                      <rdf:List>
                        <rdf:first>Small</rdf:first>
                        <rdf:rest>
                          <rdf:List>
                            <rdf:first>Marginal</rdf:first>
                            <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                          </rdf:List>
                        </rdf:rest>

                      </rdf:List>
                    </rdf:rest>

                  </rdf:List>
                </rdf:rest>

              </rdf:List>
            </rdf:rest>

          </rdf:List>
        </owl:oneOf>

      </rdfs:Datatype>
    </rdfs:range>

    <rdfs:comment>Describes the relative or perceived dimensions of objects and scenes within artwork composition. Size often correlates with narrative importance, symbolic significance, perspectival organisation, and visual hierarchy. This property captures how large something appears or functions compositionally (e.g., dominant, medium, marginal), not a physical or pixel measurement.</rdfs:comment>
    <skos:example>
            :sky_scene arto:size &quot;Dominant&quot; .
            :central_figure arto:size &quot;Large&quot; .
            :house_group   arto:size &quot;Medium&quot; .
            :small_bird    arto:size &quot;Small&quot; .
            :distant_stars arto:size &quot;Marginal&quot; .
          </skos:example>
    <rdfs:label>size</rdfs:label>
  </owl:DatatypeProperty>

  <owl:Class rdf:about="http://w3id.org/arto#Symbolism">
        <rdfs:subClassOf rdf:resource="http://w3id.org/arto#Connotation"/>
        <rdfs:subClassOf>
          <owl:Restriction>
            <owl:onProperty rdf:resource="http://w3id.org/arto#source"/>
            <owl:allValuesFrom rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
          </owl:Restriction>
        </rdfs:subClassOf>

        <rdfs:comment>Captures the symbolic meanings and iconographic content embedded in artwork elements. It connects to ICON ontology's symbolic framework while providing source property for symbolic interpretations. Symbolism instances can relate to mythological, religious, cultural, or personal symbolic systems, enabling systematic iconographic analysis and cross-cultural symbolic comparison.</rdfs:comment>
        <rdfs:label>Symbolism</rdfs:label>
        <skos:example>
    # Religious symbolism
    :dove_peace_symbolism rdf:type arto:Symbolism ;
        dc:description &quot;The dove as a symbol of peace and the Holy Spirit&quot; ;
        arto:source &quot;Christian tradition and Biblical references&quot; ;
        arto:isConnotationOf :dove_object .
        
    # Mythological symbolism
    :apple_temptation_symbolism rdf:type arto:Symbolism ;
        dc:description &quot;Apple as symbol of temptation and forbidden knowledge&quot; ;
        arto:source &quot;Biblical Genesis and Christian iconographic tradition&quot; ;
        arto:isConnotationOf :apple_object .
        
    # Cultural symbolism  
    :lotus_purity_symbolism rdf:type arto:Symbolism ;
        dc:description &quot;Lotus flower symbolising purity and spiritual awakening&quot; ;
        arto:source &quot;Buddhist and Hindu religious traditions&quot; ;
        arto:isConnotationOf :lotus_flower_object .
        
    # Historical symbolism
    :laurel_wreath_victory_symbolism rdf:type arto:Symbolism ;
        dc:description &quot;Laurel wreath as symbol of victory and honour&quot; ;
        arto:source &quot;Ancient Greek and Roman ceremonial traditions&quot; ;
        arto:isConnotationOf :laurel_crown_object .
</skos:example>
      </owl:Class>

  <owl:DatatypeProperty rdf:about="http://w3id.org/arto#source">
    <rdfs:domain rdf:resource="http://w3id.org/arto#Symbolism"/>

    <rdfs:range rdf:resource="http://www.w3.org/2001/XMLSchema#string"/>
    <rdfs:comment>Identifies the cultural, historical, or textual origins of symbolic meanings, enabling traceability of iconographic interpretations and cross-cultural symbolic analysis. Sources can include mythological traditions, religious texts, historical events, literary works, or folk traditions that inform symbolic interpretation.</rdfs:comment>
    <skos:example>
              # Religious and biblical sources
              :dove_peace_symbolism arto:source &quot;Biblical Genesis 8:11 - Noah's dove with olive branch&quot; ;
                </skos:example>
    <rdfs:label>source</rdfs:label>
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="http://www.europeana.eu/schemas/edm/isShownBy">
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/endDate">
  </owl:DatatypeProperty>

  <owl:DatatypeProperty rdf:about="https://schema.org/startDate">
  </owl:DatatypeProperty>

  <owl:Class rdf:about="http://purl.obolibrary.org/obo/MFOEM_000001">
