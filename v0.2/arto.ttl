                             dc:modified "2025-08-05" ;
                             dc:title "Artwork Object Ontology"@en ;
                             vann:preferredNamespacePrefix "arto" ;
                             rdfs:label "Artwork Object Ontology (ARTO)"@en ;
                             owl:versionInfo "0.2" .

