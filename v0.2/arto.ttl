#################################################################
#    Object Properties
#################################################################

###  http://qudt.org/schema/qudt/vector
qudt:vector rdf:type owl:ObjectProperty .


###  http://w3id.org/arto#containsElement
arto:containsElement rdf:type owl:ObjectProperty ;
                     rdfs:domain [ rdf:type owl:Class ;
                                   owl:unionOf ( arto:Object
                                                 arto:Scene
                                               )
                                 ] ;
                     rdfs:range arto:VisualElement ;
                     rdfs:comment "Specifies the visual elements (such as colour, line, composition) contained within an object in the artwork." ;
                     rdfs:label "containsElement" ;
                     skos:example """
                      - colour elements: :sky_object arto:containsElement :blue_gradient_colour
                      - Line elements: :mountain_object arto:containsElement :diagonal_line
                      - Compositional elements: :tree_object arto:containsElement :vertical_composition""" .


###  http://w3id.org/arto#containsObject
arto:containsObject rdf:type owl:ObjectProperty ;
                    owl:inverseOf arto:isObjectOf ;
                    rdfs:domain arto:Scene ;
                    rdfs:range arto:Object ;
                    rdfs:comment "Indicates specific objects that are present within a scene of an artwork." ;
                    rdfs:label "containsObject" ;
                    skos:example """
                      - Landscape scene: :garden_scene arto:containsObject :fountain, :bench, :flowers
                      - Portrait scene: :interior_scene arto:containsObject :chair, :table, :book
                      - Still life scene: :table_scene arto:containsObject :fruit_bowl, :wine_glass, :cloth""" .


###  http://w3id.org/arto#containsScene
arto:containsScene rdf:type owl:ObjectProperty ;
                   owl:inverseOf arto:isSceneOf ;
                   rdfs:domain [ rdf:type owl:Class ;
                                 owl:unionOf ( arto:Artwork
                                               arto:Scene
                                             )
                               ] ;
                   rdfs:range arto:Scene ;
                   rdfs:comment "Identifies distinct scenes within an artwork or within another scene, allowing for hierarchical scene composition." ;
                   rdfs:label "containsScene" ;
                   skos:example """
                      - Nested scenes: :market_scene arto:containsScene :vendor_scene, :customer_scene
                      - Background scenes: :landscape_painting arto:containsScene :mountain_scene, :valley_scene""" .


###  http://w3id.org/arto#currentLocation
arto:currentLocation rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf edm:currentLocation ;
                     rdfs:domain arto:Artwork ;
                     rdfs:range arto:Location ;
                     rdfs:comment "Specifies the present physical location where an artwork is housed, displayed, or stored. This property extends EDM's currentLocation with art-specific constraints ensuring single location assignment while supporting location change tracking. Essential for provenance research, exhibition planning, and artwork accessibility, this property maintains connections to institutional and geographical data through the Location class." ;
                     rdfs:label "currentLocation" ;
                     owl:cardinality 1 ;
                     skos:example """
                      - Museum location: :mona_lisa arto:currentLocation :louvre_museum
                      - Gallery location: :starry_night arto:currentLocation :moma_gallery_room_24
                      """ .


###  http://w3id.org/arto#expresses
arto:expresses rdf:type owl:ObjectProperty ;
               rdfs:domain arto:Emotion ;
               rdfs:range mfoem:MFOEM_000001 ;
               rdfs:comment "Relates arto:Emotion to a specific emotion concept defined in the Mental Functioning Ontology Emotion Module (MFOEM). This property is used to link local emotion representations to controlled vocabulary terms (e.g., joy, sadness, anger, fear) from MFOEM, ensuring consistent and interoperable emotion classification across datasets. In the context of artworks, this enables precise and standardised description of emotions expressed within artistic content." ;
               rdfs:label "expresses" ;
               skos:example """
              - :joy_emotion arto:expresses mfoem:MFOEM_000034 ; 
              - :sadness_emotion arto:expresses mfoem:MFOEM_000056 ; 
              - :anger_emotion arto:expresses mfoem:MFOEM_000009 ; 
              - :fear_emotion arto:expresses mfoem:MFOEM_000026 ; 
              """ .


###  http://w3id.org/arto#genre
arto:genre rdf:type owl:ObjectProperty ;
           rdfs:domain arto:Artwork ;
           rdfs:range arto:GenreType ;
           rdfs:comment "Genre classification is fundamental to art analysis, which connects artworks to established genre categories from Getty AAT vocabulary while supporting custom genre definitions." ;
           rdfs:label "genre" ;
           skos:example """
               - Traditional genres: :mona_lisa arto:genre aat:portrait_paintings
               - Landscape genre: :starry_night arto:genre aat:landscape_paintings  
               - Religious genre: :last_supper arto:genre aat:religious_paintings
               - Still life genre: :sunflowers arto:genre aat:still_life_paintings
               - Abstract genre: :composition_vii arto:genre aat:abstract_paintings
               """ .


###  http://w3id.org/arto#hasColourTerm
arto:hasColourTerm rdf:type owl:ObjectProperty ;
                   rdfs:domain arto:Colour ;
                   rdfs:range arto:ColourTerm ;
                   rdfs:comment "Links a colour to its corresponding standardised term from controlled vocabularies, establishing semantic connections that enable consistent colour identification, hierarchical classification, and cross-institutional interoperability for cultural heritage documentation and research." ;
                   skos:example """
                    :colour1 a arto:Colour ;
                      arto:hasColourTerm aat:red .

                    :colour2 a arto:Colour ;
                      arto:hasColourTerm aat:vermillion .

                    :colour3 a arto:Colour ;
                      arto:hasColourTerm aat:imperial_yellow .

                    """ ;
                   rdfs:label "hasColourTerm" .


###  http://w3id.org/arto#hasColourValue
arto:hasColourValue rdf:type owl:ObjectProperty ;
                    rdfs:domain arto:Colour ;
                    rdfs:range arto:ColourValue ;
                    rdfs:comment "Associates a colour with its value, allowing for flexible representation of colour information in artworks. This property supports different colour spaces for example RGB and HSV formats, enabling detailed colour analysis and representation." ;
                    skos:example """
                      - RGB value:
                        :c_vermilion a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSpace "RGB" ;
                            qudt:vector (227 66 52)      # R,G,B in 0–255
                          ] .

                      - HSV value:
                        :c_skyBlue a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSpace "HSV" ;
                            qudt:vector (197 0.76 1.00)  # H in degrees; S,V in 0–1
                          ] .

                      - CMYK value:
                        :c_cadmiumYellow a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSpace "CMYK" ;
                            qudt:vector (0.00 0.09 0.90 0.00)  # C,M,Y,K in 0–1
                          ] .

                      - CIE Lab value with white point:
                        :c_ultramarine a arto:Colour ;
                          arto:hasColourValue [
                            a arto:ColourValue ;
                            arto:colourSpace "Lab" ;
                            qudt:vector (32.4 14.8 -38.1) ;
                            arto:whitePoint arto:D65
                          ] .
                      """ ;
                    rdfs:label "hasColourValue" .


###  http://w3id.org/arto#hasConnotation
arto:hasConnotation rdf:type owl:ObjectProperty ;
                    owl:inverseOf arto:isConnotationOf ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( arto:Artwork
                                                arto:Object
                                                arto:Scene
                                                arto:VisualElement
                                              )
                                ] ;
                    rdfs:range arto:Connotation ;
                    rdfs:comment "Links visual elements, objects, scenes, or artworks to their symbolic meanings or emotional connotations.The relationship enables multiple interpretation levels while maintaining clear connections between visible elements and their cultural, symbolic, or emotional significance. Similar to ICON's recognizedConcept but with broader domain coverage." ;
                    rdfs:label "hasConnotation" ;
                    skos:example """
              - Emotional expression: :red_colour arto:hasConnotation :anger_emotion
              - Symbolic meaning: :dove_object arto:hasConnotation :peace_meaning
              - Scene mood: :storm_scene arto:hasConnotation :turbulence_emotion
              - Artwork meaning: :guernica arto:hasConnotation :anti_war_theme""" .


###  http://w3id.org/arto#hasCoordinates
arto:hasCoordinates rdf:type owl:ObjectProperty ;
                    rdfs:domain [ rdf:type owl:Class ;
                                  owl:unionOf ( arto:Object
                                                arto:Scene
                                              )
                                ] ;
                    rdfs:range qudt:Vector ;
                    rdfs:comment "Specifies the spatial location of objects and scenes within the artwork's coordinate system. This property supports spatial analysis, object relationship studies, compositional balance analysis, and digital humanities research requiring precise object localisation." ;
                    rdfs:label "hasCoordinates" ;
                    skos:example """
                        - Digital image coordinates: :main_figure arto:hasCoordinates qudt_Vector_250_300
                    qudt_Vector_250_300 rdf:type qudt:Vector ;
                        - 3D artwork coordinates: :sculpture_detail arto:hasCoordinates qudt_Vector_15_20_5
                    qudt_Vector_15_20_5 rdf:type qudt:Vector ;
                    """ .


###  http://w3id.org/arto#hasLocation
arto:hasLocation rdf:type owl:ObjectProperty ;
                 owl:inverseOf arto:isLocationOf ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( arto:Agent
                                             arto:Event
                                           )
                             ] ;
                 rdfs:range arto:Location ;
                 rdfs:comment "Specifies the physical location of an agent or event related to the artwork." ;
                 rdfs:label "hasLocation" ;
                 skos:example """
                - Museum address: :metropolitan_museum arto:hasLocation :met_nyc_location
                - Gallery location: :tate_modern arto:hasLocation :london_bankside                 
                - Exhibition event: :venice_biennale_2024 arto:hasLocation :venice_giardini""" .


###  http://w3id.org/arto#hasMetric
arto:hasMetric rdf:type owl:ObjectProperty ;
               owl:inverseOf arto:isMetricOf ;
               rdfs:domain [ rdf:type owl:Class ;
                             owl:unionOf ( arto:Connotation
                                           arto:Object
                                           arto:Scene
                                           arto:VisualElement
                                         )
                           ] ;
               rdfs:range arto:Metric ;
               rdfs:comment "Associates artwork components with quantifiable measurements supporting computational analysis and comparative studies. This property enables integration of statistical colour analysis, compositional balance metrics, emotional intensity measures, and other quantitative assessments with qualitative art historical analysis. The relationship supports both automated artwork analysis and structured comparison studies while maintaining connection to interpretive content." ;
               rdfs:label "hasMetric" ;
               skos:example """
                - Colour metrics: :sky_area arto:hasMetric :blue_intensity_75_percent
                - Composition balance: :landscape_scene arto:hasMetric :symmetry_score_0.8
                - Emotional intensity: :expression_scene arto:hasMetric :fear_intensity_high""" .


###  http://w3id.org/arto#hasTime
arto:hasTime rdf:type owl:ObjectProperty ;
             owl:inverseOf arto:isTimeOf ;
             rdfs:domain arto:Event ;
             rdfs:range arto:Time ;
              rdfs:comment "Links an event to its temporal specification. The time entity may represent a specific date, a period (e.g., season, decade), or a cultural-historical era, allowing precise or approximate temporal anchoring of events." ;
             rdfs:label "hasTime" ;
             skos:example """
               # Specific date
               :exhibition2024 arto:hasTime :date_2024_05_12

               # Period/season
               :venice_biennale2022 arto:hasTime :summer2022

               # Historical era
               :napoleon_coronation arto:hasTime :early19thCentury
              """ .


###  http://w3id.org/arto#isConnotationOf
arto:isConnotationOf rdf:type owl:ObjectProperty ;
                     rdfs:domain arto:Connotation ;
                     rdfs:range [ rdf:type owl:Class ;
                                  owl:unionOf ( arto:Artwork
                                                arto:Object
                                                arto:Scene
                                                arto:VisualElement
                                              )
                                ] ;
                     rdfs:comment "Associates a connotation with the visual elements, objects, scenes or artwork that express it." ;
                     rdfs:label "isConnotationOf" ;
                     skos:example """
                    - Emotional expression: :anger_emotion arto:isConnotationOf :red_colour
                    - Symbolic meaning: :peace_meaning arto:isConnotationOf :dove_object
                    - Scene mood: :turbulence_emotion arto:isConnotationOf :storm_scene
                    - Artwork meaning: :anti_war_theme arto:isConnotationOf :guernica""" .


###  http://w3id.org/arto#isElementOf
arto:isElementOf rdf:type owl:ObjectProperty ;
                 rdfs:domain arto:VisualElement ;
                 rdfs:range [ rdf:type owl:Class ;
                              owl:unionOf ( arto:Object
                                            arto:Scene
                                          )
                            ] ;
                 rdfs:comment "Specifies the object or scene that contains the visual element in the artwork." ;
                 rdfs:label "isElementOf" ;
                 skos:example """
                  - colour elements: :blue_gradient_colour arto:isElementOf :sky_object
                  - Line elements: :diagonal_line arto:isElementOf :mountain_object
                  - Compositional elements: :vertical_composition arto:isElementOf :tree_object""" .


###  http://w3id.org/arto#isEventOf
arto:isEventOf rdf:type owl:ObjectProperty ;
