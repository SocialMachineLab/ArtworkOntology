###  http://w3id.org/arto#Agent
arto:Agent rdf:type owl:Class ;
           rdfs:subClassOf foaf:Agent ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasLocation ;
                             owl:allValuesFrom arto:Location
                           ] ;
           rdfs:comment "Represents individuals or organisations involved in artwork creation, curation, or related activities. This class extends foaf:Agent with art-specific roles and relationships. Agents can be artists, curators, collectors, institutions, or other entities. " ;
           rdfs:label "Agent" .


###  http://w3id.org/arto#Artist
arto:Artist rdf:type owl:Class ;
            rdfs:subClassOf arto:Person ;
            rdfs:comment "The person or group who create or contribute the artwork." ;
            rdfs:label "Artist" .


###  http://w3id.org/arto#Artwork
arto:Artwork rdf:type owl:Class ;
             rdfs:subClassOf sdo:CreativeWork ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dc:contributor ;
                               owl:allValuesFrom arto:Agent
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dc:creator ;
                               owl:allValuesFrom arto:Artist
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dc:isPartOf ;
                               owl:allValuesFrom arto:Series
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:containsScene ;
                               owl:allValuesFrom arto:Scene
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:genre ;
                               owl:allValuesFrom arto:GenreType
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:hasConnotation ;
                               owl:allValuesFrom arto:Connotation
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:medium ;
                               owl:allValuesFrom arto:MediumType
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:relatedToEvent ;
                               owl:allValuesFrom arto:Event
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:style ;
                               owl:allValuesFrom arto:StyleType
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:technique ;
                               owl:allValuesFrom arto:TechniqueType
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:currentLocation ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass arto:Location
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty sdo:height ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass sdo:QuantitativeValue
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty sdo:width ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onClass sdo:QuantitativeValue
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dc:description ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty dc:title ;
                               owl:allValuesFrom xsd:string
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty edm:isShownBy ;
                               owl:maxQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                               owl:onDataRange xsd:anyURI
                             ] ;
             rdfs:comment "Represents a created object intended to be aesthetically pleasing or convey artistic meaning. This class encompasses various art forms including paintings, sculptures, photographs, and digital art. Unlike CIDOC-CRM's E22_Human-Made_Object which covers all manufactured items, arto:Artwork specifically focuses on objects created with artistic intent. The class inherits essential properties from schema:CreativeWork (creator, dateCreated, description) while adding art-specific constraints such as mandatory visual representation (edm:isShownBy) and current location tracking. Each artwork must contain at least one scene and have defined dimensions, supporting both traditional and contemporary art analysis." ;
             rdfs:label "Artwork" .


###  http://w3id.org/arto#BalanceOfComposition
arto:BalanceOfComposition rdf:type owl:Class ;
                          rdfs:subClassOf arto:Metric ;
                          rdfs:comment "Used to assess and describe the visual equilibrium of an artwork's composition. This metric captures how elements are arranged in terms of symmetry, asymmetry, and spatial distribution of visual weight. It can be applied in qualitative analysis (e.g., identifying harmonious vs. dynamic arrangements) or in quantitative evaluation when assigning a balance score to artworks." ;
                          rdfs:label "BalanceOfComposition" .


###  http://w3id.org/arto#Character
arto:Character rdf:type owl:Class ;
               rdfs:subClassOf arto:Person ;
               rdfs:comment "Represents persons, figures, or beings depicted within an artwork." ;
               rdfs:label "Character" .


###  http://w3id.org/arto#Colour
arto:Colour rdf:type owl:Class ;
            rdfs:subClassOf arto:VisualElement ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:hasColourTerm ;
                              owl:allValuesFrom arto:ColourTerm
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:hasColourValue ;
                              owl:allValuesFrom arto:ColourValue
                            ] ;
            rdfs:comment "Represents colour usage in artworks with precise technical specifications and art historical context. This class provides both computational colour analysis (RGB, HSV values) and art theoretical foundations, aligning with Getty AAT's colour concepts while maintaining specific properties for digital analysis. Colour instances can express symbolic meanings and emotional content, supporting both technical image processing and interpretive art analysis." ;
            skos:example """
            :c_vermilion a arto:Colour ;
              arto:hasColourTerm <http://vocab.getty.edu/aat/300266400> ;  # vermilion
              arto:hasColourValue [
                a arto:ColourValue ;
                arto:colourSpace "RGB" ;
                qudt:vector (227,66,52)  ] .


            :c_bluePeace a arto:Colour ;
              arto:hasColourTerm <http://vocab.getty.edu/aat/300129361> ;  # blue
              arto:hasColourValue [
                a arto:ColourValue ;
                arto:colourSpace "HSV" ;
                qudt:vector (197,76,100)
              ] ;
            """ ;
            rdfs:label "Colour" .


###  http://w3id.org/arto#ColourHistogram
arto:ColourHistogram rdf:type owl:Class ;
                     rdfs:subClassOf arto:Metric ;
                     rdfs:comment "A metric that captures the distribution of colours within an artwork. It represents how frequently each colour or colour range occurs, typically in the form of a histogram or similar quantitative structure. This metric can be used for tasks such as palette analysis, artwork comparison" ;
                     rdfs:label "ColourHistogram" .


###  http://w3id.org/arto#ColourTerm
arto:ColourTerm rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Class ;
                                  owl:unionOf ( skos:Concept
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty skos:broaderTransitive ;
                                                  owl:hasValue <http://vocab.getty.edu/aat/300131647>
                                                ]
                                              )
                                ] ;
                rdfs:comment "Represents standardised colour terminology from controlled vocabularies, art historical literature, and established colour naming systems. This class provides semantic interoperability by linking colour instances to authoritative colour classifications, enabling consistent colour identification across institutions and research projects. Colour terms encompass basic colour names, specific pigment designations, cultural colour concepts, and technical colour classifications. The class primarily aligns with Getty AAT's colour hierarchy while supporting additional colour vocabularies and historical colour terminology." ;
                rdfs:label "ColourTerm" ;
                skos:example """
                   - Basic colour terms: aat:red, aat:blue, aat:yellow, aat:green
                     # Fundamental colour categories from Getty AAT
                   - Specific pigment terms: aat:ultramarine_blue, aat:vermillion, aat:cadmium_yellow
                     # Historical and contemporary pigment names
                   - Cultural colour terms: aat:royal_purple, aat:imperial_yellow, aat:sacred_red
                     # Colours with cultural or symbolic significance
                   - Technical colour terms: aat:primary_colours, aat:complementary_colours, aat:earth_tones
                     # Colour theory and technical classifications
                   - Historical colour terms: aat:tyrian_purple, aat:lapis_lazuli_blue, aat:gold_leaf
                     # Historically significant colourants and materials
                   - Descriptive colour terms: aat:warm_colours, aat:cool_colours, aat:neutral_tones
                     # Colour temperature and mood classifications
                   """ .


###  http://w3id.org/arto#ColourValue
arto:ColourValue rdf:type owl:Class ;
                 rdfs:subClassOf [ rdf:type owl:Restriction ;
                                   owl:onProperty qudt:vector ;
                                   owl:allValuesFrom qudt:Vector
                                 ] ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty arto:colourSpace ;
                                   owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                       owl:oneOf [ rdf:type rdf:List ;
                                                                   rdf:first "CMYK" ;
                                                                   rdf:rest [ rdf:type rdf:List ;
                                                                              rdf:first "Greyscale" ;
                                                                              rdf:rest [ rdf:type rdf:List ;
                                                                                         rdf:first "HSV" ;
                                                                                         rdf:rest [ rdf:type rdf:List ;
                                                                                                    rdf:first "Lab" ;
                                                                                                    rdf:rest [ rdf:type rdf:List ;
                                                                                                               rdf:first "RGB" ;
                                                                                                               rdf:rest [ rdf:type rdf:List ;
                                                                                                                          rdf:first "XYZ" ;
                                                                                                                          rdf:rest rdf:nil
                                                                                                                        ]
                                                                                                             ]
                                                                                                  ]
                                                                                       ]
                                                                            ]
                                                                 ]
                                                     ]
                                 ] ;
                 rdfs:comment "Represents the precise numeric specification of colours using various colour models and measurement systems. This class captures quantitative colour data for digital analysis, colour matching and reproduction studies. Colour values enable objective colour comparison, automated colour analysis, and integration with digital image processing workflows. The class supports multiple colour spaces to accommodate different analytical needs and technical requirements." ;
                 rdfs:label "colourValue" ;
                 skos:example """
                    - RGB specification: :red_primary rdf:type arto:ColourValue ;
                      arto:colourSpace \"RGB\" ; qudt:vector (255,0,0) ;
                    - HSV specification: :golden_yellow rdf:type arto:ColourValue ;
                      arto:colourSpace \"HSV\" ; qudt:vector (45,85,95) ;
                    - CMYK print colours: :prussian_blue rdf:type arto:ColourValue ;
                      arto:colourSpace \"CMYK\" ; qudt:vector (100,85,0,25) ;
                    - LAB colour measurement: :skin_tone rdf:type arto:ColourValue ;
                        arto:colourSpace \"Lab\" ; qudt:vector (75,8,15) ;
                    """ .


###  http://w3id.org/arto#Composition
arto:Composition rdf:type owl:Class ;
                 rdfs:subClassOf arto:VisualElement ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty dc:type ;
                                   owl:allValuesFrom xsd:string
                                 ] ;
                 rdfs:comment """Represents the arrangement and organisation of visual elements within artworks, fundamental to art historical analysis and aesthetic evaluation. This class captures compositional strategies, balance, and spatial relationships that structure artistic meaning.
                 Currently ARTO includes common compositions types: symmetry, asymmetry, radial balance, rule of thirds, golden ratio, and dynamic composition.
                 """ ;
                 rdfs:label "Composition" .


###  http://w3id.org/arto#Connotation
arto:Connotation rdf:type owl:Class ;
                 rdfs:subClassOf crm:E28_Conceptual_Object ,
                                 [ rdf:type owl:Restriction ;
                                   owl:onProperty arto:hasMetric ;
                                   owl:allValuesFrom arto:Metric
                                 ] ;
                 rdfs:comment """Represents the implied meanings of artwork, includeing themes, symbolic meanings, and
      emotions conveyed by an artwork.""" ;
                 rdfs:label "Connotation" ;
                 skos:example """
                     - Symbolic meaning: :dove_symbolism rdf:type arto:Symbolism .
                     - Emotional content: :melancholy_mood rdf:type arto:Emotion ; arto:emotionPerspective \"artwork\"
                     - Historical reference: :revolution_allegory rdf:type arto:Connotation ;
                       dc:description \"French Revolution political symbolism\"
                     """ .


###  http://w3id.org/arto#Emotion
arto:Emotion rdf:type owl:Class ;
             rdfs:subClassOf arto:Connotation ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:expresses ;
                               owl:allValuesFrom mfoem:MFOEM_000001
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty arto:emotionPerspective ;
                               owl:allValuesFrom [ rdf:type rdfs:Datatype ;
                                                   owl:oneOf [ rdf:type rdf:List ;
                                                               rdf:first "artist" ;
                                                               rdf:rest [ rdf:type rdf:List ;
                                                                          rdf:first "artwork" ;
                                                                          rdf:rest [ rdf:type rdf:List ;
                                                                                     rdf:first "viewer" ;
                                                                                     rdf:rest rdf:nil
                                                                                   ]
                                                                        ]
                                                             ]
                                                 ]
                             ] ;
             rdfs:comment """Represents emotional content or affective responses in artworks.
             Uses MFOEM emotion ontology types and the emotionPerspective property to distinguish 
             between artist's intended emotion, artwork's emotional expression, and viewer's response.
             Emotions are expressed using standardised MFOEM emotion types including basic emotions (happiness, sadness, anger, fear, surprise, disgust) and complex emotions (love, anxiety, hope, despair, serenity). """ ;
             rdfs:label "Emotion" ;
             skos:example """
# Artist's intended emotion
:guernica_antiwar_anger  arto:Emotion ;
    arto:expresses mfoem:MFOEM_000009 ;
    arto:emotionPerspective \"artist\" ;
    dc:description \"Picasso's anger against the brutality of war\" .

# Artwork's emotional expression  
:starry_night_turbulence rdf:type arto:Emotion ;
    arto:expresses mfoem:MFOEM_000028 ;
    arto:emotionPerspective \"artwork\" ;
    dc:description \"Anxiety expressed through swirling brushstrokes and dynamic composition\" .

# Viewer's emotional response
:mona_lisa_mystery rdf:type  arto:Emotion ;
    arto:expresses mfoem:MFOEM_000167 ;
    arto:emotionPerspective \"viewer\" ;
    dc:description \"Viewer's curiosity evoked by the enigmatic smile\" .
""" .


###  http://w3id.org/arto#EmotionIntensity
arto:EmotionIntensity rdf:type owl:Class ;
                      rdfs:subClassOf arto:Metric ;
                       rdfs:comment """Represents the measured strength of an emotional response evoked by an artwork. 
Values may be expressed on different scales, for example:
- Categorical levels such as low / medium / high
- Numeric ranges (e.g., 0-1, 1-5, or 1-10)
This flexibility allows projects to choose the scale best suited to their analytical or interpretive framework.""" ;
                      rdfs:label "EmotionIntensity" .


###  http://w3id.org/arto#Event
arto:Event rdf:type owl:Class ;
           rdfs:subClassOf event:Event ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasLocation ;
                             owl:someValuesFrom arto:Location
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasTime ;
                             owl:someValuesFrom arto:Time
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty event:agent ;
                             owl:someValuesFrom arto:Agent
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:relatedToConnotation ;
                             owl:allValuesFrom arto:Connotation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty dc:type ;
                             owl:allValuesFrom xsd:string
                           ] ;
           rdfs:comment """
Represents a temporally and spatially situated occurrence involving agents and locations.
This class extends the Event Ontology and aligns conceptually with CIDOC CRM’s E5_Event,
supporting cultural heritage interoperability. Event instances may cover artistic activities
(creation, sketching, exhibition), artist life events (residence changes, collaborations),
as well as historical or mythological occurrences referenced in artworks. Event categories
can be expressed using rdf:type (e.g., Creation, Exhibition, ResidenceMove).
""" ;
      skos:example """
          << :Guernica arto:sketchedBy :Picasso >> a arto:Event ;
              arto:hasTime :t_1937_Spring ; arto:hasLocation :ParisStudio ;
              dc:type "Sketching".

          << :Guernica dcterms:creator :Picasso >> a arto:Event ;
              arto:hasTime :t_1937 ; arto:hasLocation :ParisStudio ;
              dc:type "Creation".
          << :Georgia_OKeeffe arto:movedResidenceTo :Abiquiu_Home >> a arto:Event ;
            arto:hasTime :t_1949 ; arto:hasLocation :Abiquiu_NewMexico ;
            dc:type "ResidenceMove".
          """ ;
           rdfs:label "Event" .


###  http://w3id.org/arto#GenreType
arto:GenreType rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Class ;
                                 owl:unionOf ( skos:Concept
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty skos:broaderTransitive ;
                                                 owl:hasValue <http://vocab.getty.edu/aat/300056444>
                                               ]
                                             )
                               ] ;
               rdfs:comment "Represents the genre of an artwork, such as portrait, landscape, still life, etc. This class allows for the classification of artworks based on their thematic or stylistic content." ;
               rdfs:label "GenreType" .


###  http://w3id.org/arto#Line
arto:Line rdf:type owl:Class ;
          rdfs:subClassOf arto:VisualElement ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty dc:type ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty arto:direction ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty arto:length ;
                            owl:allValuesFrom xsd:float
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty arto:width ;
                            owl:allValuesFrom xsd:float
                          ] ;
          rdfs:comment "Represents linear visual elements in artworks. Typical line types may include straight, curved, broken, contour, gestural, structural, implied, and hatching or cross-hatching. These categories are indicative and may be aligned with controlled vocabularies such as Getty AAT where applicable." ;
          rdfs:label "Line" .


###  http://w3id.org/arto#Location
arto:Location rdf:type owl:Class ;
              rdfs:subClassOf crm:E53_Place ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty geo:asWKT ;
                                owl:allValuesFrom geo:wktLiteral
                              ] ,
                              [ rdf:type owl:Restriction ;
                                owl:onProperty dc:type ;
                                owl:allValuesFrom xsd:string
                              ] ;
              rdfs:comment "Represents physical or conceptual places associated with artwork creation, display, storage, or depicted content, supporting both precise geographical coordinates and general names. This class addresses the complex relationship between location and time in historical contexts, where political boundaries and place names evolve over centuries. The class integrates GeoSPARQL coordinates with flexible place type classifications, supporting the artwork's historical geographical analysis." ;
              rdfs:label "Location" .


###  http://w3id.org/arto#MainColour
arto:MainColour rdf:type owl:Class ;
                rdfs:subClassOf arto:Metric ;
                rdfs:comment """Represents the dominant or most prominent colour in an artwork, which can be
      determined through visual analysis or colour quantisation techniques.""" ;
                rdfs:label "MainColour" .


###  http://w3id.org/arto#MaterialType
arto:MaterialType rdf:type owl:Class ;
                  rdfs:subClassOf [ rdf:type owl:Class ;
                                    owl:unionOf ( skos:Concept
                                                  [ rdf:type owl:Restriction ;
                                                    owl:onProperty skos:broaderTransitive ;
                                                    owl:hasValue <http://vocab.getty.edu/aat/300264091>
                                                  ]
                                                )
                                  ] ;
                  rdfs:comment """Represents the material type of an object, such as wood,
      metal, glass, or fabric. This class allows for the classification of objects based on the
      materials used in their construction or representation.""" ;
                  rdfs:label "MaterialType" ;
                  skos:example """
                  - :wood_material rdf:type arto:  .
                  - :metal_material rdf:type arto:MaterialType .
                  - :glass_material rdf:type arto:MaterialType .
                      """ .


###  http://w3id.org/arto#MediumType
arto:MediumType rdf:type owl:Class ;
                rdfs:subClassOf [ rdf:type owl:Class ;
                                  owl:unionOf ( skos:Concept
                                                [ rdf:type owl:Restriction ;
                                                  owl:onProperty skos:broader ;
                                                  owl:someValuesFrom <http://vocab.getty.edu/aat/300163343>
                                                ]
                                              )
                                ] ;
                rdfs:comment "Represents the medium of an artwork, such as oil on canvas, watercolour, etc. This class allows for the classification of artworks based on the materials and techniques used in their creation." ;
                rdfs:label "MediumType" .


###  http://w3id.org/arto#Metric
arto:Metric rdf:type owl:Class ;
            rdfs:comment """Represents a quantifiable measurement or characteristic of an artwork,
      encompassing statistical information about basic elements such as colour statistics, emotion
      intensity, balance of composition, and other visual elements, to facilitate structural
      analysis
      and comparison of artworks.""" ;
            rdfs:label "Metric" .


###  http://w3id.org/arto#Object
arto:Object rdf:type owl:Class ;
            rdfs:subClassOf [ rdf:type owl:Restriction ;
                              owl:onProperty arto:hasConnotation ;
                              owl:someValuesFrom arto:Connotation
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:hasCoordinates ;
                              owl:allValuesFrom qudt:Vector
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:hasMetric ;
                              owl:allValuesFrom arto:Metric
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:material ;
                              owl:allValuesFrom arto:MaterialType
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:relatedTo ;
                              owl:allValuesFrom arto:Object
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:relatedToScene ;
                              owl:allValuesFrom arto:Scene
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:state ;
                              owl:allValuesFrom skos:Concept
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:descriptor ;
                              owl:allValuesFrom xsd:string
                            ] ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:size ;
                              owl:allValuesFrom xsd:string
                            ] ;
            rdfs:comment "Represents identifiable and meaningful entities depicted within artworks, composed of various visual elements. Objects serve as the primary content units for artwork analysis, with properties describing their physical state, material composition, and spatial relationships" ;
            rdfs:label "Object" .


###  http://w3id.org/arto#Organisation
arto:Organisation rdf:type owl:Class ;
                  rdfs:subClassOf arto:Agent ,
                                  foaf:Organization ;
                  owl:disjointWith arto:Person ;
                  rdfs:comment "Represents institutional entities involved in artwork creation, curation, preservation, or display, including museums, galleries, art schools, cultural institutions, commercial art enterprises, etc." ;
                  rdfs:label "Organisation" .


###  http://w3id.org/arto#Painting
arto:Painting rdf:type owl:Class ;
              rdfs:subClassOf arto:Artwork ;
              rdfs:comment "A specific type of artwork created using paint or similar media. " ;
              rdfs:label "Painting" .


###  http://w3id.org/arto#Person
arto:Person rdf:type owl:Class ;
            rdfs:subClassOf arto:Agent ,
                            foaf:Person ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty arto:knows ;
                              owl:allValuesFrom arto:Person
                            ] ;
            rdfs:comment "Individual human beings involved with artwork may include artists, curators, critics, collectors, conservators, researchers, and other roles." ;
            rdfs:label "Person" .


###  http://w3id.org/arto#Photograph
arto:Photograph rdf:type owl:Class ;
                rdfs:subClassOf arto:Artwork ;
                rdfs:comment "A specific type of artwork created using photography or similar media. " ;
                rdfs:label "Photograph" .


###  http://w3id.org/arto#Relevance
arto:Relevance rdf:type owl:Class ;
               rdfs:subClassOf arto:Metric ;
               rdfs:comment """Measures and describes the significance or importance of elements within an artwork, including:
                  - Thematic relevance
                  - Visual importance
                  - Symbolic significance
                  - Narrative relevance
                  - Historical relevance
                  - Compositional importance""" ;
               rdfs:label "Relevance" .


###  http://w3id.org/arto#Scene
arto:Scene rdf:type owl:Class ;
           rdfs:subClassOf [ rdf:type owl:Restriction ;
                             owl:onProperty arto:containsObject ;
                             owl:allValuesFrom arto:Object
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:containsScene ;
                             owl:allValuesFrom arto:Scene
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasConnotation ;
                             owl:allValuesFrom arto:Connotation
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasCoordinates ;
                             owl:allValuesFrom qudt:Vector
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:hasMetric ;
                             owl:allValuesFrom arto:Metric
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:relatedToEvent ;
                             owl:allValuesFrom arto:Event
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:descriptor ;
                             owl:allValuesFrom xsd:string
                           ] ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty arto:size ;
                             owl:allValuesFrom xsd:string
                           ] ;
           rdfs:comment "Represents a coherent visual or narrative unit within an artwork that conveys specific meaning or sentiment. A scene aggregates objects and visual elements into meaningful compositions, supporting hierarchical structures where complex artworks contain multiple interconnected scenes. It's similar to ICON's image representations but with explicit support for scene nesting and relationships. Scenes can reference historical, mythological, or fictional events through relatedToEvent, enabling contextual artwork analysis." ;
           rdfs:label "Scene" .


###  http://w3id.org/arto#Sculpture
arto:Sculpture rdf:type owl:Class ;
               rdfs:subClassOf arto:Artwork ;
               rdfs:comment """A specific type of artwork created using sculptural techniques, such as carving,
      modelling, or casting.""" ;
               rdfs:label "Sculpture" .


###  http://w3id.org/arto#Series
arto:Series rdf:type owl:Class ;
            rdfs:subClassOf sdo:CreativeWorkSeries ,
                            [ rdf:type owl:Restriction ;
                              owl:onProperty dc:creator ;
                              owl:allValuesFrom arto:Agent
                            ] ;
            rdfs:comment """A collection of related artworks that share common characteristics, themes, or purposes, including:
                - Thematic series
                - Chronological series
                - Technique-based series
                - Subject-based series
                - Experimental series
                - Documentary series""" ;
            rdfs:label "Series" .


###  http://w3id.org/arto#Space
arto:Space rdf:type owl:Class ;
           rdfs:subClassOf arto:VisualElement ,
                           [ rdf:type owl:Restriction ;
                             owl:onProperty dc:type ;
                             owl:allValuesFrom xsd:string
                           ] ;
           rdfs:comment "Captures the representation of depth, and spatial organisation within artworks. " ;
           rdfs:label "Space" .


###  http://w3id.org/arto#Sponsor
arto:Sponsor rdf:type owl:Class ;
             rdfs:subClassOf arto:Person ;
             rdfs:comment "The person or group who sponsor the artwork." ;
             rdfs:label "Sponsor" .


###  http://w3id.org/arto#StyleType
arto:StyleType rdf:type owl:Class ;
               rdfs:subClassOf [ rdf:type owl:Class ;
                                 owl:unionOf ( skos:Concept
                                               [ rdf:type owl:Restriction ;
                                                 owl:onProperty skos:broaderTransitive ;
                                                 owl:hasValue <http://vocab.getty.edu/aat/300264088>
                                               ]
                                             )
                               ] ;
               rdfs:comment "Represents the style of an artwork, such as Impressionism, Cubism, etc. This class allows for the classification of artworks based on their stylistic characteristics." ;
               rdfs:label "StyleType" .


###  http://w3id.org/arto#Symbolism
arto:Symbolism rdf:type owl:Class ;
               rdfs:subClassOf arto:Connotation ,
                               [ rdf:type owl:Restriction ;
                                 owl:onProperty arto:source ;
                                 owl:allValuesFrom xsd:string
                               ] ;
               rdfs:comment "Captures the symbolic meanings and iconographic content embedded in artwork elements. It connects to ICON ontology's symbolic framework while providing source property for symbolic interpretations. Symbolism instances can relate to mythological, religious, cultural, or personal symbolic systems, enabling systematic iconographic analysis and cross-cultural symbolic comparison." ;
               rdfs:label "Symbolism" ;
               skos:example """
    # Religious symbolism
    :dove_peace_symbolism rdf:type arto:Symbolism ;
        dc:description \"The dove as a symbol of peace and the Holy Spirit\" ;
        arto:source \"Christian tradition and Biblical references\" ;
        arto:isConnotationOf :dove_object .
        
    # Mythological symbolism
    :apple_temptation_symbolism rdf:type arto:Symbolism ;
        dc:description \"Apple as symbol of temptation and forbidden knowledge\" ;
        arto:source \"Biblical Genesis and Christian iconographic tradition\" ;
        arto:isConnotationOf :apple_object .
        
    # Cultural symbolism  
    :lotus_purity_symbolism rdf:type arto:Symbolism ;
        dc:description \"Lotus flower symbolising purity and spiritual awakening\" ;
        arto:source \"Buddhist and Hindu religious traditions\" ;
        arto:isConnotationOf :lotus_flower_object .
        
    # Historical symbolism
    :laurel_wreath_victory_symbolism rdf:type arto:Symbolism ;
        dc:description \"Laurel wreath as symbol of victory and honor\" ;
        arto:source \"Ancient Greek and Roman ceremonial traditions\" ;
        arto:isConnotationOf :laurel_crown_object .
""" .


###  http://w3id.org/arto#TechniqueType
arto:TechniqueType rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Class ;
                                     owl:unionOf ( skos:Concept
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty skos:broaderTransitive ;
                                                     owl:hasValue <http://vocab.getty.edu/aat/300053001>
                                                   ]
                                                 )
                                   ] ;
                   rdfs:comment "Represents the specific artistic techniques used in creating artwork, drawing from Getty AAT's technique hierarchy while supporting custom undefined techniques." ;
                   rdfs:label "TechniqueType" .


###  http://w3id.org/arto#Theme
arto:Theme rdf:type owl:Class ;
           rdfs:subClassOf arto:Connotation ;
           rdfs:comment "Represents a central idea, subject, or concept explored in an artwork, which can be explicit or implicit, and may reflect broader cultural, social, or political issues." ;
           rdfs:label "Theme" .


###  http://w3id.org/arto#Time
arto:Time rdf:type owl:Class ;
          owl:equivalentClass <http://www.w3.org/2006/time#TemporalEntity> ;
          rdfs:subClassOf [ rdf:type owl:Restriction ;
                            owl:onProperty dc:type ;
                            owl:allValuesFrom xsd:string
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sdo:endDate ;
                            owl:allValuesFrom xsd:dateTime
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty sdo:startDate ;
                            owl:allValuesFrom xsd:dateTime
                          ] ;
          rdfs:comment """Represents temporal aspects related to artwork, including:
            - Creation periods
            - Historical periods
            - Exhibition periods
            - Conservation periods
            - Depicted time periods
            - Time-based elements
            """ ;
          rdfs:label "Time" ;
          skos:example """
            - :renaissance_period rdf:type arto:Time ;
                sdo:startDate \"1400\" ;
                sdo:endDate \"1600\" .
                
            """ .


###  http://w3id.org/arto#VisualElement
arto:VisualElement rdf:type owl:Class ;
                   rdfs:subClassOf [ rdf:type owl:Restriction ;
                                     owl:onProperty arto:hasConnotation ;
                                     owl:allValuesFrom arto:Connotation
                                   ] ,
                                   [ rdf:type owl:Restriction ;
                                     owl:onProperty arto:hasMetric ;
                                     owl:allValuesFrom arto:Metric
                                   ] ;
                   rdfs:comment "Represents the fundamental visual components of an artwork, including: lines and shapes, colours and textures, forms and patterns, space and perspective, light and shadow, etc." ,
                                "Represents the fundamental visual components of artworks, including colours, lines, shapes, textures, and compositional structures. This abstract class serves as the foundation for systematic visual analysis, bridging computational image processing with art historical methodology. Visual elements can express connotations and have measurable properties, supporting both quantitative analysis (colour histograms, line density) and qualitative interpretation (symbolic colour use, expressive line quality). " ;
                   rdfs:label "VisualElement" .


