###  http://w3id.org/arto#state
arto:state rdf:type owl:ObjectProperty ;
           rdfs:domain arto:Object ;
           rdfs:range skos:Concept ;
           rdfs:comment "Describes the condition, position, or status of objects within artworks, capturing both physical states (damaged, intact) and depicted conditions (motion, rest, emotional states of figures)." ;
           rdfs:label "state" ;
           skos:example """
               - Physical conditions: :broken_amphora arto:state :damaged_state ;
                 :marble_statue arto:state :weathered_state
               - Motion states: :running_horse arto:state :galloping_state ;
                 :falling_figure arto:state :in_motion_state
               - Positional states: :seated_figure arto:state :sitting_state ;
                 :reclining_venus arto:state :lying_state
               - Emotional states: :weeping_magdalene arto:state :grieving_state ;
                 :laughing_child arto:state :joyful_state
               - Temporal states: :blooming_flower arto:state :spring_state ;
                 :withered_leaves arto:state :autumn_state
               - Functional states: :open_book arto:state :readable_state ;
                 :lit_candle arto:state :burning_state
               """ .


###  http://w3id.org/arto#style
arto:style rdf:type owl:ObjectProperty ;
           rdfs:domain arto:Artwork ;
           rdfs:range arto:StyleType ;
           rdfs:comment "Indicates the artistic style or movement of an artwork, such as Impressionism, Cubism, or Baroque. This property links to Getty AAT's style and period classifications, while also allowing for the inclusion of custom style definitions when necessary." ;
           skos:example """
              :water_lilies a arto:Artwork ;
                arto:style aat:impressionism .

              :guernica a arto:Artwork ;
                arto:style aat:cubism .

              :baroque_altarpiece a arto:Artwork ;
                arto:style aat:baroque .
              """ ;
           rdfs:label "style" .


###  http://w3id.org/arto#technique
arto:technique rdf:type owl:ObjectProperty ;
               rdfs:domain arto:Artwork ;
               rdfs:range arto:TechniqueType ;
               rdfs:comment "Specifies the artistic methods or processes applied in the creation of the artwork (e.g., fresco, engraving, pointillism). This property connects to Getty AAT’s technique hierarchy, while also supporting the definition of custom techniques not covered by AAT." ;
               skos:example """
                :sistine_ceiling a arto:Artwork ;
                  arto:technique aat:fresco .

                :rembrandt_print a arto:Artwork ;
                  arto:technique aat:etching .

                :seurat_painting a arto:Artwork ;
                  arto:technique aat:pointillism .

                :bronze_sculpture a arto:Artwork ;
                  arto:technique aat:lost_wax_casting .
                """ ;
               rdfs:label "technique" .


