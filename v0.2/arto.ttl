###  http://w3id.org/arto#colourSystem
arto:colourSystem rdf:type owl:DatatypeProperty ;
                  rdfs:domain arto:ColourValue ;
                  rdfs:range [ rdf:type rdfs:Datatype ;
                               owl:oneOf [ rdf:type rdf:List ;
                                           rdf:first "CMYK" ;
                                           rdf:rest [ rdf:type rdf:List ;
                                                      rdf:first "Greyscale" ;
                                                      rdf:rest [ rdf:type rdf:List ;
                                                                 rdf:first "HSV" ;
                                                                 rdf:rest [ rdf:type rdf:List ;
                                                                            rdf:first "Lab" ;
                                                                            rdf:rest [ rdf:type rdf:List ;
                                                                                       rdf:first "RGB" ;
                                                                                       rdf:rest [ rdf:type rdf:List ;
                                                                                                  rdf:first "XYZ" ;
                                                                                                  rdf:rest rdf:nil
                                                                                                ]
                                                                                     ]
                                                                          ]
                                                               ]
                                                    ]
                                         ]
                             ] ;
                  rdfs:comment "The colour system of the ColourValue, indicating the model used for its representation. This property supports various colour models such as RGB, HSV, Lab, and CMYK, allowing for flexible colour representation and analysis in artworks. The use of a restricted set of values ensures consistency and interoperability across different systems and applications." ;
                  rdfs:label "colourSystem" .


###  http://w3id.org/arto#descriptor
arto:descriptor rdf:type owl:DatatypeProperty ;
                rdfs:domain [ rdf:type owl:Class ;
                              owl:unionOf ( arto:Object
                                            arto:Scene
                                          )
                            ] ;
                rdfs:range xsd:string ;
                rdfs:comment "Provides extensible descriptive framework for capturing object and scene characteristics not covered by specific properties, enabling domain-specific extensions through subproperties. This design addresses the infinite variety of artistic content while maintaining semantic structure. Examples include clothing descriptions for figures, or environmental conditions for scene." ;
                rdfs:label "descriptor" ;
                skos:example """
                Portrait1 :clothing :RedDress ;
                :clothing rdfs:subPropertyOf arto:descriptor""" .


###  http://w3id.org/arto#direction
arto:direction rdf:type owl:DatatypeProperty ;
               rdfs:domain arto:Line ;
               rdfs:range xsd:string ;
               rdfs:comment "Describes the orientation of linear elements in an artwork." ;
               rdfs:label "direction" .


###  http://w3id.org/arto#emotionPerspective
arto:emotionPerspective rdf:type owl:DatatypeProperty ;
                        rdfs:domain arto:Emotion ;
                        rdfs:range [ rdf:type rdfs:Datatype ;
                                     owl:oneOf [ rdf:type rdf:List ;
                                                 rdf:first "Artist" ;
                                                 rdf:rest [ rdf:type rdf:List ;
                                                            rdf:first "Artwork" ;
                                                            rdf:rest [ rdf:type rdf:List ;
                                                                       rdf:first "Viewer" ;
                                                                       rdf:rest rdf:nil
                                                                     ]
                                                          ]
                                               ]
                                   ] ;
                        rdfs:comment "Specifies the perspective or viewpoint from which an emotion is interpreted in relation to an artwork. This property distinguishes between emotions intended by the artist, emotions expressed by the artwork itself, emotions experienced by viewers. Values are restricted to: 'artist' (artist's intended emotion), 'artwork' (emotion expressed by the artwork's formal elements), and 'viewer' (viewer's emotional response)." ;
                        rdfs:label "emotionPerspective"@en ;
                        skos:example """
                          ex:melancholy_emotion arto:emotionPerspective \"artist\"^^xsd:string ;
                          ex:serenity_emotion arto:emotionPerspective \"artwork\"^^xsd:string ;
                          ex:excitement_emotion arto:emotionPerspective \"viewer\"^^xsd:string .
                          """ .


###  http://w3id.org/arto#length
arto:length rdf:type owl:DatatypeProperty ;
            rdfs:domain arto:Line ;
            rdfs:range xsd:float ;
            rdfs:comment "The length of the line." ;
            rdfs:label "length" .


###  http://w3id.org/arto#size
arto:size rdf:type owl:DatatypeProperty ;
          rdfs:domain [ rdf:type owl:Class ;
                        owl:unionOf ( arto:Object
                                      arto:Scene
                                    )
                      ] ;
          rdfs:range xsd:string ;
          rdfs:comment "Indicates the relative or absolute dimensions of objects and scenes within artwork composition. Size often correlate with narrative importance, symbolic significance, or perspectival organisation in artistic composition." ;
          rdfs:label "size" .


###  http://w3id.org/arto#source
arto:source rdf:type owl:DatatypeProperty ;
            rdfs:domain arto:Symbolism ;
            rdfs:range xsd:string ;
            rdfs:comment "Identifies the cultural, historical, or textual origins of symbolic meanings, enabling traceability of iconographic interpretations and cross-cultural symbolic analysis. Sources can include mythological traditions, religious texts, historical events, literary works, or folk traditions that inform symbolic interpretation." ;
            rdfs:label "source" .


###  http://w3id.org/arto#width
arto:width rdf:type owl:DatatypeProperty ;
            rdfs:domain arto:Line ;
            rdfs:range xsd:float ;
            rdfs:comment "The width of the line." ;
            rdfs:label "width" .


