###  http://w3id.org/arto#MaterialType
arto:MaterialType rdf:type owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:comment """Represents materials used in artwork creation. This class encompasses both AAT (Art & Architecture Thesaurus) material concepts from the Getty vocabulary (300264091 - Materials Facet) and custom material concepts defined locally. Examples include oil paint, canvas, bronze, marble, watercolor, etc."""@en ;
    owl:equivalentClass [
        owl:unionOf (
            [ owl:hasValue aat:300264091 ] # AAT Materials Facet
            skos:Concept        # Custom materials
        )
    ] .


arto:ArtStyleType rdf:type owl:Class ;
    rdfs:subClassOf skos:Concept ;
    rdfs:comment """Represents artistic styles used to classify artworks. This class encompasses both AAT (Art & Architecture Thesaurus) style concepts from the Getty vocabulary (300015646 - Styles Facet) and custom style concepts defined locally. Examples include Renaissance, Baroque, Impressionism, Abstract Expressionism, etc."""@en ;
    owl:equivalentClass [
        owl:unionOf (
            [ owl:hasValue aat:300015646 ] # AAT Styles Facet
            skos:Concept           # Custom styles
        )
    ] .

